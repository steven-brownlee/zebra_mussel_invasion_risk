	author = {Bereket, Isaac N and de Loë, Rob},
	month = oct,
	year = {2020},
	note = {Place: Montpellier
Publisher: Water Alternatives Association},
	keywords = {Canada, Discourse Analysis, Eutrophication, Governance, Inland waters, Lake, Lake basins, Lake Erie, Lakes, Phosphorus, Policies, Political Science--International Relations, Storylines, Usa, Water Policy, Water quality, Watersheds},
	pages = {800--821},
