Publisher: Nature Publishing Group},
	keywords = {Cellulase, Cellulose, Climate, Climate change, Diet, Fish, Herbivore, Herbivores, Herbivory, Latitude, Microorganisms, Organic matter, Prey, Sciences: Comprehensive Works, Species richness, Temperature, Temperature effects, Water temperature},
	pages = {39600},
