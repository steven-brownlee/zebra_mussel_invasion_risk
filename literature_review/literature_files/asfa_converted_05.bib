	annote = {Last updated - 2023-11-21},
	annote = {SubjectsTermNotLitGenreText - Gibbula umbilicalis; Asparagopsis armata; United States--US},
}

@article{noauthor_impact_2021,
	title = {Impact of {Marine} {Aquaculture} on the {Microbiome} {Associated} with {Nearby} {Holobionts}: {The} {Case} of {Patella} caerulea {Living} in {Proximity} of {Sea} {Bream} {Aquaculture} {Cages}},
	volume = {9},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/scholarly-journals/impact-marine-aquaculture-on-microbiome/docview/2493893860/se-2?accountid=13800},
	doi = {10.3390/microorganisms9020455},
	abstract = {Aquaculture plays a major role in the coastal economy of the Mediterranean Sea. This raises the issue of the impact of fish cages on the surrounding environment. Here, we explore the impact of aquaculture on the composition of the digestive gland microbiome of a representative locally dwelling wild holobiont, the grazer gastropod Patella caerulea, at an aquaculture facility located in Southern Sicily, Italy. The microbiome was assessed in individuals collected on sea bream aquaculture cages and on a rocky coastal tract located about 1.2 km from the cages, as the control site. Patella caerulea microbiome variations were explained in the broad marine metacommunity context, assessing the water and sediment microbiome composition at both sites, and characterizing the microbiome associated with the farmed sea bream. The P. caerulea digestive gland microbiome at the aquaculture site was characterized by a lower diversity, the loss of microorganisms sensitive to heavy metal contamination, and by the acquisition of fish pathogens and parasites. However, we also observed possible adaptive responses of the P. caerulea digestive gland microbiome at the aquaculture site, including the acquisition of putative bacteria able to deal with metal and sulfide accumulation, highlighting the inherent microbiome potential to drive the host acclimation to stressful conditions.},
	language = {English},
	number = {2},
	journal = {Microorganisms},
	year = {2021},
	note = {Place: Basel
Publisher: MDPI AG},
	keywords = {16S rRNA gene sequencing, Acclimation, Acclimatization, aquaculture, Aquaculture, Aquaculture facilities, Biology--Microbiology, Bream, Cages, Composition, Contamination, Digestive glands, Farms, Fish, Fish parasites, gastropoda, Heavy metals, Holobiont, Marine aquaculture, marine holobionts, Mediterranean Sea, microbiome, Microbiome, Microbiomes, Microorganisms, patella, Patella caerulea, Salinity, Seawater, Sedimentation \& deposition, Sediments, Skin, Software, Sparidae, Sulfide},
