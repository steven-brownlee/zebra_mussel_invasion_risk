Publisher: MDPI AG},
	keywords = {Aquatic ecosystem, Biology, cancer, cyanobacteria, Cyanobacteria, Cyanotoxin, cyanotoxins, Health, proliferation, toxicity},
	pages = {1896--1917},
	annote = {Copyright - Copyright MDPI AG 2013},
	annote = {Last updated - 2023-12-05},
}

@article{volker_comparative_2013,
	title = {Comparative {Toxicity} {Assessment} of {Nanosilver} on {Three} {Daphnia} {Species} in {Acute}, {Chronic} and {Multi}-{Generation} {Experiments}},
	volume = {8},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/scholarly-journals/comparative-toxicity-assessment-nanosilver-on/docview/1440074765/se-2?accountid=13800},
	doi = {10.1371/journal.pone.0075026},
	abstract = {The antibacterial properties of nanosilver have led to a versatile application spectrum including medical purposes and personal care products. However, the increasing use of nanosilver has raised concerns about its environmental impacts. Long-term exposure studies with aquatic invertebrates are essential to assess possible adverse effects on aquatic ecosystems. In the present study, acute (48 h), chronic (21 d) and long-term effects of nanosilver (primary size 15 nm) on five successive generations of three Daphnia species (D. magna, D. pulex, and D. galeata) were investigated. Acute EC50 values of nanosilver were 121 µg Ag L−1 for D. magna being the least sensitive species and 8.95 and 13.9 µg Ag L−1 for D. pulex and D. galeata, respectively. Chronic exposure provided EC10 values of 0.92 µg Ag L−1 for D. magna showing the most sensitive chronic reaction and 2.25 and 3.45 µg Ag L−1 for D. pulex and D. galeata, respectively. Comparative exposure to AgNO3 revealed a generally higher toxicity of the soluble form of silver. The multi-generation experiments resulted in effects on the population level for all tested species. Exposure of D. magna indicated an increased toxicity of nanosilver in the fifth generation of animals exposed to 10 µg Ag L−1. Neonates from pre-exposed parental daphnids did not completely recover when transferred into clean water. Exposure of D. pulex and D. galeata revealed not only increasing toxicity in some generations, but also greater tolerance to nanosilver. This study contributes to the assessment of the risk potential of nanosilver on aquatic ecosystems. It shows that effects of nanosilver vary within one genus and change with exposure duration. Therefore, long-term studies considering different aquatic species are needed to better understand the possible effects of nanosilver on aquatic ecosystems.},
	language = {English},
	number = {10},
	journal = {PLoS One},
	author = {Völker, Carolin and Boedicker, Cathinka and Daubenthaler, Jan and Oetken, Matthias and Oehlmann, Jörg},
	month = oct,
	year = {2013},
	note = {Place: San Francisco
Publisher: Public Library of Science},
	keywords = {Aquatic ecosystem, Aquatic ecosystems, Aquatic organisms, Chemical wastewater, Chronic exposure, Consumer products, Copper, Daphnia, Daphnia magna, Death rates, EC50, Ecosystem assessment, Ecosystems, Environmental changes, Environmental impact, Environmental issues, Experiments, Exposure, Germany, Invertebrates, Long-term effects, Nanomaterials, Nanoparticles, Neonates, Newborn babies, Particle size, Sciences: Comprehensive Works, Silver, Silver nanoparticle, Species, Toxicity},
