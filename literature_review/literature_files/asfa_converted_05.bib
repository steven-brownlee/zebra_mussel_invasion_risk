@article{yang_spatial_2020,
	title = {Spatial {Distribution} of {Toxic} {Metal}(loid)s and {Microbial} {Community} {Analysis} in {Soil} {Vertical} {Profile} at an {Abandoned} {Nonferrous} {Metal} {Smelting} {Site}},
