@article{carella_nuclear_2017,
	title = {Nuclear morphometry and ploidy of normal and neoplastic haemocytes in mussels},
	volume = {12},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/scholarly-journals/nuclear-morphometry-ploidy-normal-neoplastic/docview/1876058266/se-2?accountid=13800},
	doi = {10.1371/journal.pone.0173219},
	abstract = {Haemic neoplasia (HN) in bivalves has been reported in association with mass mortality events in various species of molluscs. The aim of this work was to quantify the nuclear morphometry and DNA content of neoplastic cells of mussels Mytilus galloprovincialis affected by HN using nuclear densitometry in Feulgen-stained preparations. The results were also compared with a population of normal mussel haemocytes. We captured 256 images of 3 different neoplasia stages and 120 images of normal haemocytes; thus, a total of 120,166 nuclei were analysed. We extracted 21 morphological parameters from normal and neoplastic nuclei. Eighteen of these parameters were different (P{\textless}0.05). Among those (expressed in pixel units—inter-pixel distance of 0.45 micrometres—as: normal vs. neoplastic) nuclear area (117.1±94.1 vs. 423.1±226.9), perimeter (44.9±14.0 vs. 79.0±21.3) and (IOD) integrated optical density (13.47±34.5 vs. 177.1±150.8) were relevant features to discriminate between normal and neoplastic cells. Those differences allowed identifying two distinctive populations of neoplastic nuclei, occasionally in the same individuals at a given phase of the disease. Moreover, neoplastic haemocytes in less extended lesions showed a ploidy value of 6.2 n along with the presence of a second population of circulating cells with a DNA content of 10.7n. In samples with moderate disease only one peak at 7n was observed. Finally, in more severe conditions, a further ploidy peak of 7.8n was recorded, accompanied by a shallow but broad peak of 31n. This latter extreme value is thought to be due to the presence of giant multinucleated cells where individual nuclei overlap in space and cannot be discerned individually. Computer-based imaging allowed the direct visualization of the cell populations and simultaneous collection of ploidy data as well as morphological features of nuclei.},
	language = {English},
	number = {3},
	journal = {PLoS One},
	author = {Carella, Francesca and De Vico, Gionata and Landini, Gabriel},
	month = mar,
	year = {2017},
	note = {Place: San Francisco
Publisher: Public Library of Science},
	keywords = {B cells, Biology, Bivalves, Bivalvia, Cancer, Cell division, Chemical contaminants, Densitometers, Densitometry, Deoxyribonucleic acid--DNA, Extreme values, Feulgen staining, Flow cytometry, Hemocytes, Histology, Lesions, Mediterranean mussel, Microscopy, Mollusks, Morphology, Morphometry, Mussel, Mussels, Mytilus edulis, Mytilus galloprovincialis, Nuclei, Nuclei (cytology), Optical density, Pathogenesis, Pixels, Ploidy, Populations, Sciences: Comprehensive Works, Studies, Venerupis},
	annote = {Copyright - © 2017 Carella et al. This is an open access article distributed under the terms of the Creative Commons Attribution License: http://creativecommons.org/licenses/by/4.0/ (the “License”), which permits unrestricted use, distribution, and reproduction in any medium, provided the original author and source are credited. Notwithstanding the ProQuest Terms and Conditions, you may use this content in accordance with the terms of the License.},
