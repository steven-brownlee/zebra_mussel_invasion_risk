@article{noauthor_high-throughput_2021,
	title = {High-{Throughput} {Screening} of {Psychotropic} {Compounds}: {Impacts} on {Swimming} {Behaviours} in {Artemia} franciscana},
	volume = {9},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/scholarly-journals/high-throughput-screening-psychotropic-compounds/docview/2503376361/se-2?accountid=13800},
	doi = {10.3390/toxics9030064},
	abstract = {Animal behaviour is becoming increasingly popular as an endpoint in ecotoxicology due to its increased sensitivity and speed compared to traditional endpoints. However, the widespread use of animal behaviours in environmental risk assessment is currently hindered by a lack of optimisation and standardisation of behavioural assays for model species. In this study, assays to assess swimming speed were developed for a model crustacean species, the brine shrimp Artemia franciscana. Preliminary works were performed to determine optimal arena size for this species, and weather lux used in the experiments had an impact on the animals phototactic response. Swimming speed was significantly lower in the smallest arena, whilst no difference was observed between the two larger arenas, suggesting that the small arena was limiting swimming ability. No significant difference was observed in attraction to light between high and low light intensities. Arena size had a significant impact on phototaxis behaviours. Large arenas resulted in animals spending more time in the light side of the arena compared to medium and small, irrespective of light intensity. The swimming speed assay was then used to expose specimens to a range of psychotropic compounds with varying modes of action. Results indicate that swimming speed provides a valid measure of the impacts of behaviour modulating compounds on A. franciscana. The psychotropic compounds tested varied in their impacts on animal behaviour. Fluoxetine resulted in increased swimming speed as has been found in other crustacean species, whilst oxazepam, venlafaxine and amitriptyline had no significant impacts on the behaviours measured. The results from this study suggest a simple, fast, high throughput assay for A. franciscana and gains insight on the impacts of a range of psychotropic compounds on the swimming behaviours of a model crustacean species used in ecotoxicology studies.},
	language = {English},
	number = {3},
	journal = {Toxics},
	year = {2021},
	note = {Place: Basel
Publisher: MDPI AG},
	keywords = {Amitriptyline, Animal behavior, Animals, Arenas, artemia, Artemia, Artemia franciscana, Assaying, Behavior, behaviour, behavioural ecotoxicology, Biology, Brine shrimp, Crustacean, Crustaceans, Cysts, ecotoxicology, Ecotoxicology, Environmental assessment, Environmental risk, Ethology, Fluoxetine, High-throughput screening, Light, Light intensity, Luminous intensity, Optimization, Phototaxis, Pollutants, psychotropics, Risk assessment, Risk taking, Species, Swimming, Swimming behavior, Venlafaxine, Weather},
	pages = {64},
	annote = {Copyright - © 2021. This work is licensed under http://creativecommons.org/licenses/by/3.0/  (the “License”).  Notwithstanding the ProQuest Terms and Conditions, you may use this content in accordance with the terms of the License.},
