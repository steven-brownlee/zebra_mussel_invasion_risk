	journal = {Applied Sciences},
	author = {Pietropaoli, Marco and Tofani, Silvia and Formato, Giovanni and Rubino, Roberta Carlotta and Pietrella, Gabriele and {Camilla Di Ruggiero} and Milito, Marcella and Merola, Carmine and Amorena, Michele and Cersini, Antonella},
