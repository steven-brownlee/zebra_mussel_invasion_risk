@phdthesis{walters_fluctuating_1998,
	address = {Canada -- British Columbia, CA},
	type = {Ph.{D}.},
	title = {Fluctuating asymmetry and its relation to health and personality pathology},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/dissertations-theses/fluctuating-asymmetry-relation-health-personality/docview/304479382/se-2?accountid=13800},
	abstract = {Developmental stability refers to the ability to compensate for developmental upsets and produce an ideal phenotype. Fluctuating asymmetry (FA) is used as a measure of developmental stability. FA is a population-level pattern of deviations from bilateral symmetry. If a trait manifests FA, then asymmetry in individuals may be used as a reflection of the ability to buffer against developmental upsets. The literature on FA in humans and other animals is reviewed, with reference to a number of fundamental conceptual and empirical issues that affect the analysis and use of FA. Several ANOVA strategies for the analysis of FA are reviewed and a general framework for conceptualizing and analyzing FA is developed. The special problems presented by discrete data are discussed, and a strategy for analyzing asymmetry in fingerprints that combines data from fingerprint ridge counts and patterns is proposed. A method for statistically testing whether a number of asymmetries may be summed is presented. Data were collected from undergraduates (n = 145) on bilateral skeletal, facial, and fingerprint traits, health, personality pathology and intrasexual competitiveness. Not all asymmetries met the criteria for FA. In those that did, in general, there were relatively few relationships between individual asymmetries, or between asymmetry in individuals and health, personality pathology or competitiveness. No statistical justification was found for using composite asymmetry scores for sets of traits measured in the present study. Discussion focusses on comparing present results with previous findings, outlining possible limitations of the current study, evaluating the usefulness of particular bilateral traits in FA research, and suggesting directions for future studies.},
	language = {English},
	school = {Simon Fraser University (Canada)},
	author = {Walters, Sally},
