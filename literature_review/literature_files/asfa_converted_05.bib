Publisher: MDPI AG},
	keywords = {Biology--Biochemistry, Coastal environments, copper, Copper, Discriminant analysis, Energy metabolism, Enzymes, Europe, High-performance liquid chromatography, Immune system, Marina, Mass spectroscopy, Metabolic pathways, Metabolism, Metabolites, metabolomics, Metabolomics, Mimachlamys varia, Organisms, Osmoregulation, Oxidative stress, Pollution, scallop, Scallop, UHPLC/QToF mass spectrometry},
