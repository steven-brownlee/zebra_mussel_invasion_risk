	keywords = {Abundance, Baltic Sea, Beach seines, Biology--Zoology, Biomass, Catch per unit effort, Commercial fishing, Density, Females, Fish, Fisheries, Fishing, Gillnets, Gulf of Finland, Introduced species, Invasive species, Juveniles, Marine fishes, Neogobius melanostomus, Offshore, Perciformes, Population, Population characteristics, Population number, Population size, Relative abundance, Round goby, Sex ratio, Shallow water},
	pages = {327--337},
