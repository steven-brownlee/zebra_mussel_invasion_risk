@article{reis_conservation_2024,
	title = {Conservation {Challenges} {Imposed} by {Evolutionary} {History} and {Habitat} {Suitability} {Shifts} of {Endangered} {Freshwater} {Mussels} under a {Global} {Climate} {Change} {Scenario}},
	volume = {16},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/scholarly-journals/conservation-challenges-imposed-evolutionary/docview/3046828771/se-2?accountid=13800},
	doi = {10.3390/d16040209},
	abstract = {Climate change and associated shifts in temperature and precipitation patterns have become an increasing concern as drivers of ongoing biodiversity loss. The Mediterranean region is particularly vulnerable, being both a biodiversity hotspot and a region very prone to desertification. Freshwater mussels are amongst the most threatened invertebrate taxa worldwide. Unio tumidiformis is an endemic and endangered species restricted to the southern Iberian Peninsula, living in temporary Mediterranean-type streams. Freshwater mussels need a fish host for successful larval transformation, meaning U. tumidiformis must belong to the genus Squalius. The main objective of this study was to evaluate the vulnerability of U. tumidiformis to climate change, by studying its population genetics and evolutionary history, its current and future habitat suitability, and that of its hosts. Genetic population structure and diversity were assessed using Single-Nucleotide Polymorphisms through Genotyping by Sequencing and used to infer species evolutionary history. The species potential distribution was modeled using an ensemble forecasting approach, and future shifts in habitat suitability were assessed with the projected climate data layers from Worldclim. Most populations showed extreme genetic differentiation (Fst up to 0.745), even from close neighboring ones. Upper Guadiana populations were more diverse and less differentiated. We hypothesize that U. tumidiformis originated in Upper Guadiana and followed the same colonization routes as their hosts with numerous founder effects and bottlenecks. Our results also predicted a reduction of 99\% of climatically suitable areas for U. tumidiformis in the Iberian Peninsula until 2040. For the fish hosts, a maximum 42\% reduction in suitable areas was estimated throughout the century, with remaining adequate habitats in the north. Our results suggest that difficult conservation options are necessary, prioritizing the preservation of populations, translocations to the northern area of its historical range and stream engineering to increase resilience to droughts.},
	language = {English},
	number = {4},
	journal = {Diversity},
	author = {Reis, Joaquim and Perea, Silvia and Gama, Mafalda and Mendes, Sofia L and Sousa, Vitor C and Lima, Cristina and Banha, Filipe and Gil, Maria and {Maria Garcia Alvarez} and Anastácio, Pedro and {Carla Sousa Santos} and Araujo, Rafael},
	year = {2024},
	note = {Place: Basel
Publisher: MDPI AG},
	keywords = {Biodiversity, Biodiversity hot spots, Biodiversity loss, Climate change, Climate change scenarios, Climatic data, Conservation, Desertification, distribution modeling, drought, Drought, Endangered \& extinct species, Endangered species, Endemic species, Ensemble forecasting, Evolution, Evolutionary genetics, Fish, Freshwater, Freshwater fish, Freshwater molluscs, genetic population structure, Genetic transformation, Genetics, Genotyping, Geographical distribution, Global climate, Habitats, History, Hosts, Iberia, Iberian Peninsula, Inland water environment, Mediterranean-type temporary stream, Mollusks, Mussels, Nucleotides, Population genetics, Population structure, Populations, Precipitation patterns, Rare species, rescue, Single-nucleotide polymorphism, Translocation},
