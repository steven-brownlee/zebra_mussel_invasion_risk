@article{berezina_environmental_2023,
	title = {Environmental {Assessment} with {Cage} {Exposure} in the {Neva} {Estuary}, {Baltic} {Sea}: {Metal} {Bioaccumulation} and {Physiologic} {Activity} of {Bivalve} {Molluscs}},
	volume = {11},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/scholarly-journals/environmental-assessment-with-cage-exposure-neva/docview/2869437564/se-2?accountid=13800},
	doi = {10.3390/jmse11091756},
	abstract = {The rise in anthropogenic impacts on the marine environment requires new water management. The use of a triadic approach (bioaccumulation, bioassay, and physiological biomarkers) has been shown to have good applicability for the comparative assessment of the environmental state of the Neva Estuary (Gulf of Finland, Baltic Sea). The novelty of the methodological approach of the study was that it involved both active and passive bio-monitoring methods for assessing the quality of estuarine environment. The classical analyses of metal concentration in bottom sediments, in field biota (fish and molluscs), and in caged molluscs were accompanied by a bioassay of sediment toxicity using amphipods. The physiological state of molluscs kept in cages was assessed according to two functional characteristics, such as cardio-tolerance and metabolic activity (oxygen consumption rate), after exposition in cages. The method of active monitoring (caging exposure with molluscs) as a measurement of parameters in clean molluscs has proven itself well in controlling the accumulation of both metals and oil products. Macroalgae that are abundant in estuarine ecosystems are also good indicators of metals, at least showing the transition from bottom sediments to the next level of food webs. Unionid molluscs were found to be a more sensitive and effective indicator of contaminant accumulation than dreissenid molluscs, characterized by a low tolerance to changeable environmental conditions in the estuarine ecosystem and rather high mortality in cages. Our results have shown that caging exposure with unionids can be a widely used methodological approach for the assessment of estuarine environmental quality through the determination of metal concentrations in molluscs and their physiological state.},
	language = {English},
	number = {9},
	journal = {Journal of Marine Science and Engineering},
	author = {Berezina, Nadezhda and Maximov, Alexey and Sharov, Andrey and Gubelit, Yulia and Kholodkevich, Sergei},
	year = {2023},
	note = {Place: Basel
Publisher: MDPI AG},
	keywords = {Accumulation, Algae, Animals, Anthropogenic factors, Aquatic crustaceans, Arsenic, Baltic Sea, Bio-assays, Bioaccumulation, bioaccumulation factor, bioassay, Bioassays, Biomarkers, Biomonitoring, Biota, Bottom sediments, Brackishwater environment, Cages, Carcinogens, cardioactivity, Contaminants, Earth Sciences--Oceanography, Environmental assessment, Environmental conditions, Environmental Impact Assessment, Environmental monitoring, Environmental quality, Estuaries, Estuarine dynamics, Estuarine ecosystems, Estuarine environments, Exposure, Fish, Food chains, Food webs, Gulf of Finland, Hazardous materials, Heart rate, heavy metals, Heavy metals, Human influences, Marine environment, Metal concentrations, Metals, Mollusca, Mollusks, Monitoring methods, Neva River, Nuclear power plants, Organisms, oxygen consumption, Oxygen consumption, Physiology, Pollutants, Pollution, pond mussel, Quality assessment, Sediment, Sediments, Shellfish, Toxicity, Water management, zebra mussel},
