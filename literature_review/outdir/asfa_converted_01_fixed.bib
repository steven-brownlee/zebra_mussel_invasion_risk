@article{mackay_non-native_1991,
	address = {Toronto, Ont.},
	title = {Non-native species provide big threat to existing habitats: [{SU2} {Edition}]},
	issn = {03190781},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/newspapers/non-native-species-provide-big-threat-existing/docview/436350866/se-2?accountid=13800},
	abstract = {While the mute swan, the swan species most likely to be encountered along Toronto's waterfront, appears to be here to stay, if a co-ordinated effort were made in the relatively small number of provinces and states where the species has become established, it could be slowly and humanely eliminated, without a single bird being killed, by sterilizing the eggs.                 The other species I see as a potential threat to the environment is the red deer. Like the mute swan (plus the starling, pigeon, house sparrow, house mouse, purple loosestrife and zebra mussel) the red deer is native to Europe. It's kept on game farms in the province to provide venison to restaurants. It's a species with a track record of being able to establish itself in other regions, where its abundance has caused it to become a hated nuisance.                 We didn't know any better 101 years ago, but we should know better now, and yet we constantly run risks. I don't want to see our native wildlife replaced by non-native species. I don't want to see people hating red deer and mute swans. I don't want to see animal protectionists and conservationists fighting each other over relatively small differences in value systems, as has happened over the issue of culling deer threatening rare vegetation in isolated refugia in southwestern Ontario.},
	language = {English},
	journal = {Toronto Star},
	author = {MacKay, Barry Kent},
	month = feb,
	year = {1991},
	keywords = {General Interest Periodicals--Canada},
	pages = {D7},
