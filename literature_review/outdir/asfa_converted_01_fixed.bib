@article{prescott_use_2014,
	title = {Use of the calcite saturation index as an indicator of environmental suitability for dreissenid mussels},
	volume = {5},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/scholarly-journals/use-calcite-saturation-index-as-indicator/docview/2287017449/se-2?accountid=13800},
