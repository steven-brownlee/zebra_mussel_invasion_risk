Publisher: Regional Euro-Asian Biological Invasions Centre},
	keywords = {Biology, Blacklisting, Dreissena polymorpha, Ecosystems, Eriocheir sinensis, Europe, Introduced species, Invasive species, Predation, Predators, Prey},
	pages = {341--350},
