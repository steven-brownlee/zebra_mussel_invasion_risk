Publisher: Hindawi Limited},
	keywords = {Continuous time systems, Cooperation, Dynamic stability, Dynamical systems, Ecologists, Ecology, Ecosystems, Food chains, Gestation, Hopf bifurcation, Liapunov functions, Mathematical models, Mathematics, Mutualism, Orbits, Predation, Predator-prey simulation, Predators, Research, Teams, Time lag},
