	keywords = {Biology, Deoxyribonucleic acid--DNA, Edna, Environmental DNA, Invasive species, Mississippi River, Mollusks, Montana, Mussel, Plankton, Reproduction, Rivers, United States--US, Water temperature, Zebra mussel},
