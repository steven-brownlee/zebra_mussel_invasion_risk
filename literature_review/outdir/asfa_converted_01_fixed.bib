	number = {6},
	journal = {International Journal of Environmental Research and Public Health},
	author = {Blackburn, Jason K and Ganz, Holly H and {José Miguel Ponciano} and Turner, Wendy C and Ryan, Sadie J and Kamath, Pauline and Cizauskas, Carrie and Kausrud, Kyrre and Holt, Robert D and Stenseth, Nils Chr and Getz, Wayne M},
