	keywords = {Biology, Deoxyribonucleic acid--DNA, Dreissena bugensis, Environmental DNA, Introduced species, Invasive species, Invertebrates, Mollusks, Nonnative species, Polymerase chain reaction, Rare species, Rivers, Sampling, Surveillance},
