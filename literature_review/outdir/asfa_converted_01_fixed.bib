@article{jackson_population-level_2012,
	title = {Population-{Level} {Metrics} of {Trophic} {Structure} {Based} on {Stable} {Isotopes} and {Their} {Application} to {Invasion} {Ecology}},
	volume = {7},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/scholarly-journals/population-level-metrics-trophic-structure-based/docview/1323859238/se-2?accountid=13800},
	doi = {10.1371/journal.pone.0031757},
	abstract = {Biological invasions are a significant driver of human-induced global change and many ecosystems sustain sympatric invaders. Interactions occurring among these invaders have important implications for ecosystem structure and functioning, yet they are poorly understood. Here we apply newly developed metrics derived from stable isotope data to provide quantitative measures of trophic diversity within populations or species. We then use these to test the hypothesis that sympatric invaders belonging to the same functional feeding group occupy a smaller isotopic niche than their allopatric counterparts. Two introduced, globally important, benthic omnivores, Louisiana swamp crayfish (Procambarus clarkii) and carp (Cyprinus carpio), are sympatric in Lake Naivasha, Kenya. We applied our metrics to an 8-year data set encompassing the establishment of carp in the lake. We found a strong asymmetric interaction between the two invasive populations, as indicated by inverse correlations between carp abundance and measures of crayfish trophic diversity. Lack of isotopic niche overlap between carp and crayfish in the majority of years indicated a predominantly indirect interaction. We suggest that carp-induced habitat alteration reduced the diversity of crayfish prey, resulting in a reduction in the dietary niche of crayfish. Stable isotopes provide an integrated signal of diet over space and time, offering an appropriate scale for the study of population niches, but few isotope studies have retained the often insightful information revealed by variability among individuals in isotope values. Our population metrics incorporate such variation, are robust to the vagaries of sample size and are a useful additional tool to reveal subtle dietary interactions among species. Although we have demonstrated their applicability specifically using a detailed temporal dataset of species invasion in a lake, they have a wide array of potential ecological applications.},
	language = {English},
	number = {2},
	journal = {PLoS One},
	author = {Jackson, Michelle C and Donohue, Ian and Jackson, Andrew L and Britton, J Robert and Harper, David M and Grey, Jonathan},
	month = feb,
	year = {2012},
	note = {Place: San Francisco
Publisher: Public Library of Science},
	keywords = {Aquatic plants, Biodiversity, Biology, Carp, Carps, Competition, Correlation analysis, Crayfish, Cyprinus carpio, Data processing, Diet, Ecological niches, Ecology, Ecosystem biology, Ecosystem structure, Ecosystems, England, Environmental changes, Fish, Fisheries, Fishing, Human influences, Hypotheses, Invasive species, Ireland, Isotope studies, Isotopes, Lake Naivasha, Lakes, Multivariate analysis, Niche overlap, Niches, Nonnative species, Omnivores, Population studies, Populations, Predation, Prey, Procambarus clarkii, Salmonidae, Sciences: Comprehensive Works, Species diversity, Stable isotope ratio, Stable isotopes, Statistical methods, Sulfur, Sympatric populations, Trophic, Trophic interactions, United Kingdom--UK, Variance analysis},
