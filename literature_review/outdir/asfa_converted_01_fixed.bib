@article{eliso_polystyrene_2024,
	title = {Polystyrene {Micro}- and {Nanoplastics} ({PS} {MNPs}): {A} {Review} of {Recent} {Advances} in the {Use} of -{Omics} in {PS} {MNP} {Toxicity} {Studies} on {Aquatic} {Organisms}},
	volume = {9},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/scholarly-journals/polystyrene-micro-nanoplastics-ps-mnps-review/docview/3001520371/se-2?accountid=13800},
	doi = {10.3390/fishes9030098},
	abstract = {In recent years, micro- and nanoplastics (MNPs) have represented an emerging threat for the aquatic environment due to their persistence and widespread distribution. Indeed, their small size and increased surface area lead to a high biological reactivity, which can be crucial for the health status of biota. To date, several studies have investigated lethal and sublethal endpoints of MNPs, while one of the main challenges is to understand their mechanisms of toxicity in a comprehensive manner. With this aim, the field of aquatic ecotoxicology can now benefit from the -omics approaches. This review focuses on the recent advances related to the use of transcriptomics, proteomics, and metabolomics to deeply investigate the molecular and biochemical responses of aquatic organisms, both fishes and aquatic invertebrates, to pristine polystyrene (PS) MNPs. The literature reviewed revealed that transcriptomics and metabolomics are the most frequently used -omic approaches. Overall, the studies taken into consideration shed light on the events triggered by PS MNPs at molecular and cellular levels, identifying as mechanisms of toxicity the pathways involved in oxidative stress, energy metabolism, immune response, and the nervous system. Future studies should therefore focus on the use and integration of multi-omics approaches for a more comprehensive understanding of the mechanisms involved in MNPs toxicity.},
	language = {English},
	number = {3},
	journal = {Fishes},
	author = {Eliso, Maria Concetta and Billè, Barbara and Cappello, Tiziana and Maisano, Maria},
	year = {2024},
	note = {Place: Basel
Publisher: MDPI AG},
	keywords = {-omics approaches, Aquatic environment, aquatic invertebrates, Aquatic invertebrates, Aquatic organisms, Biodegradation, Biology--Zoology, Biota, Defence mechanisms, Ecotoxicology, Embryos, Energy metabolism, fish, Fish, Genes, Genomes, Immune response, Immunity, Invertebrates, mechanism of toxicity, Metabolism, Metabolites, metabolomics, Metabolomics, microplastics, nanoplastics, Nervous system, Organisms, Oxidative metabolism, Oxidative stress, polystyrene, Polystyrene, Proteins, proteomics, Proteomics, Toxicity, transcriptomics, Transcriptomics},
