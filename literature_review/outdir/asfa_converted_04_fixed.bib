@phdthesis{lambert_developmental_2021,
	address = {United States -- Florida},
	type = {Ph.{D}.},
	title = {Developmental {Toxicity} of {Select} {Ultraviolet}-{Filters} to {Daphnia} magna and {Their} {Potential} {Role} as {Invertebrate} {Endocrine} {Disruptors}},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/dissertations-theses/developmental-toxicity-select-ultraviolet-filters/docview/2586445821/se-2?accountid=13800},
	abstract = {Ultraviolet (UV) filters are emerging contaminants of concern found ubiquitously in the aquatic environment. Many organic UV filters are endocrine disrupting compounds (EDCs) in vertebrates. However, few studies have assessed their effects on invertebrates. Ecdysis, or the shedding of the exoskeleton, is proposed to be a target of these compounds in the largest phylum of invertebrates, Arthropoda (insects and crustaceans). Ecdysis is regulated by an arthropod specific endocrine system, the ecdysteroid pathway, and alterations of this signaling cascade by EDCs results in improper development, reduced growth, or death. These outcomes may have significant implications for organismal and population health. In this dissertation, I test the overarching hypothesis that UV filters are endocrine disruptors of the ecdysteroid pathway in Daphnia magna by evaluating their effects on molting, development, and gene expression. In Chapter 2, I show that Daphnia magna chronically exposed to OMC, BP3, and 4MBC produce offspring with developmental abnormalities. Additionally, qPCR and RNAseq data demonstrate that BP3 and 4MBC affect the transcription of ecdysteroid-regulated genes. In Chapter 3, cellular transactivation assays are used to demonstrate that only one of these compounds, OMC, causes transactivation of the EcR. However, this only occurs in the presence of the endogenous hormone, indicating that a separate mechanism besides direct EcR agonism is likely for activation. In Chapter 4, OMC toxicity is further investigated using an ecdysteroid-responsive cell line. Several potential mechanisms of toxicity for this UV filter are identified using RNAseq analysis, including a possible connection to the juvenile hormone pathway. Taken together, these results suggest that UV filters appear to be both vertebrate and invertebrate endocrine disruptors. However, the concentrations needed to elicit toxicity are well above those levels detected in the environment and therefore the risk posed to aquatic invertebrates, such as Daphnia, is likely low based on the endpoints measured.},
	language = {English},
	school = {University of Florida},
	author = {Lambert, Faith Nichole},
	collaborator = {Vulpe, Chris D.},
	year = {2021},
	note = {ISBN: 9798492753894
Publication Title: ProQuest Dissertations and Theses
28264307},
	keywords = {0383:Toxicology, 0409:Endocrinology, 0768:Environmental science, Daphnia magna, Endocrinology, Environmental science, Invertebrates, Toxicity, Toxicology, Ultraviolet (UV) filters, Vertebrates},
