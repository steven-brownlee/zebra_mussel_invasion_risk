Publisher: MDPI AG},
	keywords = {Acclimation, Acclimatization, Alkalinity, Biology--Zoology, Calcium carbonate, Design, Experiments, Fight-or-flight response, Fish, Fish hatcheries, Fish stocking, Freshwater fishes, Glucose, Hard water, hardness, Hardness, Mortality, Nonnative species, Oncorhynchus mykiss, rainbow trout, Rainbow trout, Restocking, Salmon, South Dakota, Stress response, Survival, tempering, Trout, United States--US, Water hardness, water quality, Water softening, Water temperature},
	pages = {44},
