	note = {Place: Almería
Publisher: Regional Euro-Asian Biological Invasions Centre},
	keywords = {Biology, Invasions, Invasive species, Landscape, Mapping, Pests, Propagation, Spatial distribution, Stochasticity, Temporal distribution},
