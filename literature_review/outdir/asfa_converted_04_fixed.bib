Publisher: MDPI AG},
	keywords = {Bacteria, Biomonitors, Biosensor, Biosensors, Chemistry--Analytical Chemistry, Drinking water, Genetic modification, Genotoxicity, Indicator organisms, Laboratories, Luminescence, Luminescent bacteria, Monitoring, Mussels, Sensors, Sol-gel processes, Surface water, Temperature control, Water intoxication, Water quality},
