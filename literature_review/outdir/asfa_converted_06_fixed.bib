Publisher: MDPI AG},
	keywords = {Biology--Biochemistry, Chromatography, Contaminants, Contamination, Electron transport, Enrichment, Exposure, Gas chromatography, Liquid chromatography, Mass spectrometry, Mass spectroscopy, Metabolism, Metabolites, Metabolomics, Methionine, Mitochondria, Procladius, Random forest, Scientific imaging, Sediments, Sugar, Toxicity, Urea, Wetland, Wetlands},
