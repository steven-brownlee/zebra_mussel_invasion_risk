}

@article{reguera_dinophysis_2014,
	title = {Dinophysis {Toxins}: {Causative} {Organisms}, {Distribution} and {Fate} in {Shellfish}},
	volume = {12},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/scholarly-journals/dinophysis-toxins-causative-organisms/docview/1536901278/se-2?accountid=13800},
	doi = {10.3390/md12010394},
	abstract = {Several Dinophysis species produce diarrhoetic toxins (okadaic acid and dinophysistoxins) and pectenotoxins, and cause gastointestinal illness, Diarrhetic Shellfish Poisoning (DSP), even at low cell densities ({\textless}103 cells*L-1). They are the main threat, in terms of days of harvesting bans, to aquaculture in Northern Japan, Chile, and Europe. Toxicity and toxin profiles are very variable, more between strains than species. The distribution of DSP events mirrors that of shellfish production areas that have implemented toxin regulations, otherwise misinterpreted as bacterial or viral contamination. Field observations and laboratory experiments have shown that most of the toxins produced by Dinophysis are released into the medium, raising questions about the ecological role of extracelular toxins and their potential uptake by shellfish. Shellfish contamination results from a complex balance between food selection, adsorption, species-specific enzymatic transformations, and allometric processes. Highest risk areas are those combining Dinophysis strains with high cell content of okadaates, aquaculture with predominance of mytilids (good accumulators of toxins), and consumers who frequently include mussels in their diet. Regions including pectenotoxins in their regulated phycotoxins will suffer from much longer harvesting bans and from disloyal competition with production areas where these toxins have been deregulated.},
	language = {English},
	number = {1},
	journal = {Marine Drugs},
	author = {Reguera, Beatriz and Riobó, Pilar and Rodríguez, Francisco and Díaz, Patricio A and Pizarro, Gemita and Paz, Beatriz and Franco, Jose M and Blanco, Juan},
	year = {2014},
	note = {Place: Basel
Publisher: MDPI AG},
	keywords = {Destiny, diarrhoetic shellfish poisoning, diarrhoetic shellfish toxins, Dinophysis, Distribution, DSP, DSP distribution and impacts, harmful algal blooms, pectenotoxins, Pharmacy And Pharmacology, Shellfish, Shellfish poisoning},
	pages = {394--461},
