@article{konecna_population_2010,
	title = {Population and individual consequences of breeding resource availability in the {European} bitterling ({Rhodeus} amarus)},
	volume = {64},
	issn = {0340-5443},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/scholarly-journals/population-individual-consequences-breeding/docview/375565080/se-2?accountid=13800},
	doi = {10.1007/s00265-010-0921-4},
	abstract = {Resource availability may affect both individual fitness and population demography and the effects can interact. We used two experiments to test how breeding resource abundance and its spatial distribution, combined with female abundance, affected male reproductive behavior, population spawning rate, and embryo development and recruitment in the European bitterling (Rhodeus amarus), a small cyprinid fish that lays its eggs in living unionid mussels. In the first experiment, we found that at the population level the abundance of breeding resources (freshwater mussels) was more important for bitterling recruitment than resource spatial distribution (clumped or regular). In contrast at the individual level, (variability in reproductive success) the spatial distribution of resources was more important, but only when resource abundance was not limiting. Territorial males obtained almost exclusive access to fertilizations when resources were abundant and distributed regularly, but were unable to defend large clusters of resources (when rival abundance was always high) and abandoned territoriality. Surprisingly, territorial males remained aggressive and successfully defended their territories when resources were grouped into a single cluster, but at a low abundance. In the second experiment, more rapid embryo development and larger juvenile body size at the end of the growing season was detected at high resource abundance and low female abundance, indicating that early hatched juveniles survived better and hence investment in offspring production early in the season yields a higher fitness pay-offs. The abundance of females in spawning condition was the best overall predictor of the intensity of male reproductive behavior in both experiments.[PUBLICATION ABSTRACT]},
	language = {English},
	number = {7},
	journal = {Behavioral Ecology and Sociobiology},
	author = {Konecná, Markéta and Smith, Carl and Reichard, Martin},
	month = jul,
	year = {2010},
	note = {Place: Heidelberg
Publisher: Springer Nature B.V.},
	keywords = {Acheilognathinae, Animal populations, Animal reproduction, Fish, Males, Psychology, Reproductive success, Rhodeus},
	pages = {1069--1079},
	annote = {Copyright - Springer-Verlag 2010},
	annote = {Document feature - References},
	annote = {Last updated - 2024-03-21},
}

