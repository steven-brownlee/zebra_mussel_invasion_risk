@article{kattner_what_2021,
	title = {What {Animal} {Cancers} teach us about {Human} {Biology}},
	volume = {11},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/scholarly-journals/what-animal-cancers-teach-us-about-human-biology/docview/2598229618/se-2?accountid=13800},
	doi = {10.7150/thno.56623},
	abstract = {Cancers in animals present a large, underutilized reservoir of biomedical information with critical implication for human oncology and medicine in general. Discussing two distinct areas of tumour biology in non-human hosts, we highlight the importance of these findings for our current understanding of cancer, before proposing a coordinated strategy to harvest biomedical information from non-human resources and translate it into a clinical setting.
First, infectious cancers that can be transmitted as allografts between individual hosts, have been identified in four distinct, unrelated groups, dogs, Tasmanian devils, Syrian hamsters and, surprisingly, marine bivalves. These malignancies might hold the key to improving our understanding of the interaction between tumour cell and immune system and, thus, allow us to devise novel treatment strategies that enhance anti-cancer immunosurveillance, as well as suggesting more effective organ and stem cell transplantation strategies. The existence of these malignancies also highlights the need for increased scrutiny when considering the existence of infectious cancers in humans.
Second, it has long been understood that no linear relationship exists between the number of cells within an organism and the cancer incidence rate. To resolve what is known as Peto's Paradox, additional anticancer strategies within different species have to be postulated. These naturally occurring idiosyncrasies to avoid carcinogenesis represent novel potential therapeutic strategies.},
	language = {English},
	number = {14},
	journal = {Theranostics},
	author = {Kattner, Patricia and Zeiler, Katharina and Herbener, Verena J and {Katia La Ferla-Brühl} and Kassubek, Rebecca and Grunert, Michael and Burster, Timo and Brühl, Oliver and Weber, Anna Sarah and Strobel, Hannah and Karpel-Massler, Georg and Ott, Sibylle and Hagedorn, Alexa and Tews, Daniel and Schulz, Ansgar and Prasad, Vikas and Siegelin, Markus D and Nonnenmacher, Lisa and Fischer-Posovszky, Pamela and {Marc-Eric Halatsch} and {Klaus-Michael Debatin} and {Mike-Andrew Westhoff}},
