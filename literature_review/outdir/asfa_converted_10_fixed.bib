	number = {3},
	journal = {Toronto Life},
	month = mar,
	year = {2004},
	note = {Place: Toronto
Publisher: St. Joseph Communications},
	keywords = {General Interest Periodicals--Canada, Tourism, Ontario Canada, 9172:Canada, Resorts \& spas, Vacations},
	pages = {Insert 3--118},
	annote = {Copyright - Copyright Toronto Life Publishing Company Mar 2004},
	annote = {Document feature - Illustrations; Maps},
