@book{oostendorp_phonological_2000,
	address = {Berlin/Boston, Berlin/Boston},
	title = {Phonological {Projection}: {A} {Theory} of {Feature} {Content} and {Prosodic} {Structure}},
	isbn = {978-3-11-080924-4},
