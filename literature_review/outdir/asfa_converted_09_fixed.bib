@article{langton_alien_2006,
	address = {Toronto, Ont.},
	title = {Alien invader from {North} {America}; {Eastern} grey squirrel at home in {Europe} {Taken} to {England} as a novelty in late 19th century: [{ONT} {Edition}]},
	issn = {03190781},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/newspapers/alien-invader-north-america-eastern-grey-squirrel/docview/438971033/se-2?accountid=13800},
	abstract = {Widely considered cute and virtually always the first wild animal encountered by most of us, Eastern grey squirrels generally blend into the background, except when they're begging for bread crumbs. Of course, they're not without their bad habits. Eastern grey squirrels - which are commonly black in many areas, especially southern Ontario - are expert at raiding bird feeders and will occasionally search inside poorly covered garbage cans and bags for a quick meal, but are generally considered more curious than notorious.                 Grey squirrels don't just outcompete red squirrels, which are about one-third their size, they also take their nest holes and kill their young. Worse yet, grey squirrels carry parapoxvirus, a disease also known as squirrel pox, which is harmless in greys but often fatal in reds. The overall result has been the greys have largely replaced reds wherever greys exist. According to a 1995 study commissioned by the European Squirrel Initiative, a watchdog group representing national governments and universities, there were about 2 million Eastern grey squirrels in England and no more than 30,000 reds.                 "All data gathered in Britain and Italy confirm that in areas colonized by the grey squirrel, the native red squirrel rapidly becomes extinct," says Piero Genovesi, a professor at the University of Bologna, a researcher with the Italian Wildlife Institute and an expert on the squirrel invasion. "In Piedmont, the grey squirrel had been confined until 1970 within the suburbs of Turin in Stupinigi Park where the red squirrel was also present. The last observation of a red squirrel there was recorded during the winter of 1979-80."},
	language = {English},
	journal = {Toronto Star},
