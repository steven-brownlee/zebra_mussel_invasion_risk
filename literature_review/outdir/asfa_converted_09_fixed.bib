
@article{pla_switches_2022,
	title = {Switches, stability and reversals: the evolutionary history of sexual systems in fish},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/working-papers/switches-stability-reversals-evolutionary-history/docview/2505702302/se-2?accountid=13800},
	doi = {10.1101/2021.01.25.428070},
	abstract = {Sexual systems are highly diverse and have profound consequences for population dynamics and resilience. Yet, little is known about how they evolved. Using phylogenetic Bayesian modelling and a sample of 4614 species, we show that gonochorism is the likely ancestral condition in teleost fish. While all hermaphroditic forms revert quickly to gonochorism, protogyny and simultaneous hermaphroditism are evolutionarily more stable than protandry. In line with theoretical expectations, simultaneous hermaphroditism does not evolve directly from gonochorism but can evolve slowly from sequential hermaphroditism, particularly protandry. We find support for the predictions from life history theory that protogynous, but not protandrous, species live longer than gonochoristic species and invest the least in male gonad mass. The distribution of sexual systems of teleosts on the tree of life does not seem to reflect just adaptive predictions, suggesting that adaptations alone may not fully explain why some sexual forms evolve in some taxa but not others (Williams' paradox). We propose that future studies should incorporate mating systems, spawning behaviours, and the diversity of sex determining mechanisms. Some of the latter might constrain the evolution of hermaphroditism, while the non-duality of the embryological origin of teleost gonads might explain why protogyny predominates over protandry in teleosts.
Competing Interest Statement
The authors have declared no competing interest.
Footnotes
* We have revised the assignment of the sexual system of all hermaphrodites and thus updated our dataset. We consider hermaphrodites only the species for which functional hermaphroditism has been proved in the primary literature. Details can be found in the Materials \& Methods section, and an overview of all the process of data acquisition and curation is presented the new Suppl. Fig. 1, which also indicates the source of the phylogenetic tree used. Mating systems and spawning behavior definitions have been improved and clarified. Some of the predictions of life history theory have been clarified and all the analyses are now corrected for allometry. Thus, - figures 2, 4 and 5 revised. - tables 2 and 3 revised. - new figure 3 uploaded. Most of the conclusions of our study hold although some results using this new dataset change slightly. Specifically, we still find that: i) both types of sequential hermaphroditism can evolve slowly from gonochorism and revert quickly to it; ii) protogyny is lost at low rate to protandry and simultaneous hermaphroditism; iii) protandry is not evolutionarily stable and is quickly lost to gonochorism, protogyny and, to a lesser degree, simultaneous hermaphroditism. However, we now find that simultaneous hermaphroditism does not evolve directly from gonochorism but via sequential hermaphroditism, most likely protandry, thus confirming theoretical predictions. In addition, simultaneous hermaphroditism is more likely to be lost to gonochorism than to either form of sequential hermaphroditism. Analyses on life history traits confirm that protogynous (but not protandrous) species live longer than gonochoristic and present the smallest GSI. With the new dataset we no longer find evidence that protandrous species are smaller and mature later than protogynous species.},
	language = {English},
	journal = {bioRxiv},
	author = {Pla, Susanna and Benvenuto, Chiara and Capellini, Isabella and Piferrer, Francesc},
	year = {2022},
	note = {Place: Cold Spring Harbor},
	keywords = {Population dynamics, Biology, Species, Evolution, Gonads, Life history, Phylogeny, Bayesian analysis, Males, Hermaphroditism, Protandry},
NA
	annote = {Last updated - 2022-02-17},
}

