* https://marinegenomics.oist.jp/symb/viewer/info?project\_id=40
* https://www.ncbi.nlm.nih.gov/bioproject?term=PRJNA627564\&cmd=DetailsSearch},
	language = {English},
	journal = {bioRxiv},
	author = {Ripken, Christina and Khalturin, Konstantin and Shoguchi, Eiichi},
	year = {2020},
	note = {Place: Cold Spring Harbor},
	keywords = {Biology, Photosynthesis, Plastics, Marine ecosystems, Marine organisms, Coral reefs, Microorganisms, Gene expression, Oceans, Polystyrene, Dinoflagellates, Symbionts, Mitosis, Dynein, Cell number, Cladocopium, Dinoflagellata},
NA
	annote = {Last updated - 2021-04-03},
}

