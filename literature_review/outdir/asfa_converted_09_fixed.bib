@article{hentchel_genome-scale_2018,
	title = {Genome-scale fitness profile of {Caulobacter} crescentus grown in natural freshwater},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/working-papers/genome-scale-fitness-profile-caulobacter/docview/2068988560/se-2?accountid=13800},
	doi = {10.1101/279711},
	abstract = {Bacterial genomes evolve in complex ecosystems and are best understood in this natural context, but replicating such conditions in the lab is challenging. We used transposon sequencing to define the fitness consequences of gene disruption in the bacterium Caulobacter crescentus grown in natural freshwater, compared to axenic growth in common laboratory media. Gene disruptions in amino acid and nucleotide sugar pathways and in metabolic substrate transport machinery impaired fitness in both lake water and defined minimal medium relative to complex peptone broth. Fitness in lake water was enhanced by insertions in genes required for flagellum biosynthesis and reduced by insertions in genes involved in biosynthesis of the holdfast surface adhesin. We further uncovered numerous hypothetical and uncharacterized genes for which disruption impaired fitness in lake water, defined minimal medium, or both. At the genome scale, the fitness profile of mutants cultivated in lake water was more similar to that in complex peptone broth than in defined minimal medium. Microfiltration of lake water did not significantly affect the terminal cell density or the fitness profile of the transposon mutant pool, suggesting that Caulobacter does not strongly interact with other microbes in this ecosystem on the measured timescale. Fitness of select mutants with defects in cell surface biosynthesis and environmental sensing were significantly more variable in lake water than in defined medium, presumably owing to day-to-day heterogeneity in the lake environment. This study reveals genetic interactions between Caulobacter and a natural freshwater environment, and provides a new avenue to study gene function in complex ecosystems.
Footnotes
* This manuscript has been revised in response to feedback from reviewers during peer review, and in response to feedback from colleagues who read the original version of the manuscript on BiorXiv. We have clarified descriptions of data analysis, revised figures, added additional analyses to the supplemental tables, and conducted new analyses of the data (which are reflected in the revised text and the figures).},
	language = {English},
	journal = {bioRxiv},
	author = {Hentchel, Kristy L and Reyes Ruiz, Leila M and Fiebig, Aretha and Curtis, Patrick D and Coleman, Maureen L and Crosson, Sean},
	year = {2018},
	note = {Place: Cold Spring Harbor},
	keywords = {Lakes, Biology, Ecosystems, Freshwater ecosystems, Biosynthesis, Genes, Genomes, Freshwater environments, Amino acids, Feedback, Sugar, Reproductive fitness, Cell surface, Transposons, Cell density, Culture media, Flagella, Gene disruption, Peptones},
