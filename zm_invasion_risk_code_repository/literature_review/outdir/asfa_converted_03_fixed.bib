	keywords = {Anguilla anguilla, Biodiversity, Biology, Cyprinus carpio, Food availability, Food sources, Freshwater ecosystems, Freshwater lakes, Introduced species, Isotopes, Micropterus salmoides, Phragmites australis, Predation, Predators, Procambarus clarkii, Reintroduction, Species composition},
