	keywords = {9172:Canada, Biology--Botany, Exotic plant, Flowers \& plants, Forests, Habitat fragmentation, Habitats, Invasive species, Nonnative species, Plant community, Prince Edward Island Canada, Protected area, Research, Vascular plant, Wildlife conservation},
