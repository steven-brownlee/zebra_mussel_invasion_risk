	note = {Place: Heidelberg
Publisher: BioMed Central},
	keywords = {Biodiversity, Coasts, Earth Sciences--Oceanography, Ecosystem engineer, Ecosystems, Invasive species, Marine sediment, Nonnative species, Sediment, Sediments, Studies, Trawling},
	pages = {95--106},
	annote = {Copyright - Springer-Verlag and AWI 2009},
