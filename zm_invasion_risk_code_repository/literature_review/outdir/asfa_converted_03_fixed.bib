@book{rotherham_invasive_2011,
	address = {London, London},
	title = {Invasive and {Introduced} {Plants} and {Animals}: {Human} {Perceptions}, {Attitudes} and {Approaches} to {Management}},
	isbn = {978-1-134-06195-2},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/books/invasive-introduced-plants-animals/docview/2131985371/se-2?accountid=13800},
	abstract = {There have been many well-publicized cases of invasive species of plants and animals, often introduced unintentionally but sometimes on purpose, causing widespread ecological havoc. Examples of such alien invasions include pernicious weeds such as Japanese knotweed, an introduced garden ornamental which can grow through concrete, the water hyacinth which has choked tropical waterways, and many introduced animals which have out-competed and displaced local fauna. This book addresses the broader context of invasive and exotic species, in terms of the perceived threats and environmental concerns which surround alien species and ecological invasions. As a result of unprecedented scales of environmental change, combined with rapid globalisation, the mixing of cultures and diversity, and fears over biosecurity and bioterrorism, the known impacts of particular invasions have been catastrophic. However, as several chapters show, reactions to some exotic species, and the justifications for interventions in certain situations, including biological control by introduced natural enemies, rest uncomfortably with social reactions to ethnic cleansing and persecution perpetrated across the globe. The role of democracy in deciding and determining environmental policy is another emerging issue. In an increasingly multicultural society this raises huge questions of ethics and choice. At the same time, in order to redress major ecological losses, the science of reintroduction of native species has also come to the fore, and is widely accepted by many in nature conservation. However, with questions of where and when, and with what species or even species analogues, reintroductions are acceptable, the topic is hotly debated. Again, it is shown that many decisions are based on values and perceptions rather than objective science. Including a wide range of case studies from around the world, his book raises critical issues to stimulate a much wider debate.},
	language = {English},
	publisher = {Taylor \& Francis Group, Taylor \& Francis Group},
	editor = {Rotherham, Ian D. and Lambert, Robert A.},
	month = jun,
	year = {2011},
	note = {Publication Title: Invasive and Introduced Plants and Animals},
	keywords = {Botany, Ecology, Environmental conditions, Introduced organisms, Life sciences, Perceptions, RNCB, RNK, SCI011000:SCIENCE / Life Sciences / Botany, SCI020000:SCIENCE / Life Sciences / Ecology, SCI070000:SCIENCE / Life Sciences / Zoology / General, Science, Zoology},
