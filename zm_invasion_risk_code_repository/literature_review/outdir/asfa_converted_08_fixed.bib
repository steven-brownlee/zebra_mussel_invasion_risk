* https://doi.org/doi:10.25345/C5F84T},
	language = {English},
	journal = {bioRxiv},
	author = {Clark, Chase M and Hernandez, Antonio and Mullowney, Michael W and Fitz-Henley, Jhewelle and Li, Emma and Romanowski, Sean B and Pronzato, Roberto and Manconi, Renata and Murphy, Brian T},
	year = {2021},
	note = {Place: Cold Spring Harbor},
	keywords = {Biology, Mass spectroscopy, Species, Geographical distribution, Bacteria, Bioinformatics, Phylogeny, Microbiomes},
NA
	annote = {Last updated - 2023-11-09},
}

