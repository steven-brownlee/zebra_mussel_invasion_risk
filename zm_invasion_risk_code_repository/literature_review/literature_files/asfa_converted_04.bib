@phdthesis{ha_water_2021,
	address = {United States -- Indiana},
	type = {Ph.{D}.},
	title = {Water {Quality} and {Land} {Use} in {Some} {Midwestern} {Regions} {Such} as {Madison} ({WI}) and {Terre} {Haute} ({IN}) {Analyzed} with {Multispectral}/{Hyperspectral} {Remote} {Sensing}},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/dissertations-theses/water-quality-land-use-some-midwestern-regions/docview/2638939009/se-2?accountid=13800},
	abstract = {Fertilizers play a key role in agricultural production, but they also contribute to the severity of environmental problems. When agricultural fields are adjacent to streams or lakes, the overuse of fertilizers from agricultural practices can lead to eutrophication and algal blooms (i.e., Yahara lake chain, WI). In this research, empirical regression analysis was applied using remotely-sensed (RS) data, including Landsat 5, 7, and 8, and Sentinel-2, to predict eutrophication using a time series for the Yahara lake chain from 1984–2020. Supervised image analysis was applied to hyperspectral (HS) data (i.e., AVIRIS) to map the various phytoplankton classes, which were characterized as harmful (cyanobacteria \& blue-green algae) versus other harmless phytoplankton groups. Finally, the efficiency of object-based image analysis (OBIA) and RS data fusion was tested using open-source tools such as QGIS \& Orfeo-toolbox to improve land use \& land cover (LULC) mapping.Regression models derived from Landsat sensors (5-TM, 7-ETM, and 8-OLI) and Sentinel-2 revealed that despite many measures to mitigate the amount of nutrients added to the lakes, the levels of lake’s eutrophication in the Yahara lake chain were getting worse from 1984–2020. The atmospheric correction models, including ACOLITE and C2RCC, derived the comparable outputs of chlorophyll-a concentration from various remotely sensed data of different sensors acquired on the same days. In the 2009-2011 period, the algal bloom (mainly cyanobacteria and blue-green algae) was mainly distributed near the center of Lake Mendota, leaving the water clearer near the shoreline.},
	language = {English},
	school = {Indiana State University},
	author = {Ha, Hung Q.},
	collaborator = {Stone, Jeffery R. and Aldrich, Stephen P.},
	year = {2021},
