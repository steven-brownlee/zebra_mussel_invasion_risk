}

@article{morbey_sizeselective_2018,
	title = {Size‐selective fishing and the potential for fisheries‐induced evolution in lake whitefish},
	volume = {11},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/scholarly-journals/size-selective-fishing-potential-fisheries/docview/2089816300/se-2?accountid=13800},
	doi = {10.1111/eva.12635},
	abstract = {The long‐term evolutionary effects of fishing on maturation schedules can depend on gear type, the shape of the gear type's size‐selectivity function, and the size and age structure of a population. Our goal was to better understand how environmentally induced differences in somatic growth influence the evolutionary effects of size‐selective fisheries, using lake whitefish (Coregonus clupeaformis) in Lake Huron as a case study. Using a state‐dependent optimization model of energy allocation parameterized for lake whitefish, we show that fishing with gill nets (bell‐shaped selectivity) and trap nets (sigmoid‐shaped selectivity) can be potent agents of selection on size thresholds for maturity. Compared to trap nets and large mesh (114 mm) gill nets, small mesh (89 mm) gill nets are better able to buffer populations from fishing‐induced evolution by safeguarding large, fecund fish, but only when overall fishing mortality is low and growth rates sufficiently fast such that fish can outgrow vulnerable size classes. Regardless of gear type, and all else being equal, high fishing mortality in combination with low growth rates is expected to intensify the long‐term evolutionary effects of fishing.},
	language = {English},
	number = {8},
	journal = {Evolutionary Applications},
	author = {Morbey, Yolanda E and {Marin Mema}},
	month = sep,
	year = {2018},
	note = {Place: Hoboken
Publisher: John Wiley \& Sons, Inc.},
	keywords = {Age composition, Biology, Coregonus clupeaformis, Evolution, Fisheries, fisheries‐induced evolution, Fishery, Fishing, freshwater fisheries, growth rate, Growth rate, Lake Huron, Lake whitefish, life history evolution, maturation reaction norm, Mortality, Nets, Whitefish},
	pages = {1412--1424},
	annote = {Copyright - © 2018. This work is published under http://creativecommons.org/licenses/by/4.0/ (the “License”).  Notwithstanding the ProQuest Terms and Conditions, you may use this content in accordance with the terms of the License.},
