Publisher: EDP Sciences},
	keywords = {Benthic communities, Benthic zone, Benthos, Boats, Deployment, Distribution, Diving, Drifters, Environmental Studies, essais, Experimental design, expérimentation de terrain, field experiment, Field tests, Floats, Freshwater, Freshwater ecosystems, Global Positioning System, hydrophytes, Inland water environment, Locating, macroinvertebrates, macroinvertébrés, Marine ecosystems, Permits, Plateaus, Retrieval, Rope, sédiment, sediments, Substrates, Terrain, trays, Trays},
	annote = {Copyright - © 2017. This work is licensed under http://creativecommons.org/licenses/by-nd/4.0/  (the “License”).  Notwithstanding the ProQuest Terms and conditions, you may use this content in accordance with the terms of the License.},
