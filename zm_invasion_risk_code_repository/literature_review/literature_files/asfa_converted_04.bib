	keywords = {Active control, Adductor muscle, Aquaculture, biological early warning system, Bivalvia, Body condition, Condition monitoring, Controlled conditions, Data analysis, Disturbances, dynamic model, Dynamics, Early warning system, Early warning systems, Earth Sciences--Oceanography, ecophysiology, Ecosystem disturbance, Marine molluscs, Measurement techniques, Mollusks, Monitoring systems, Muscles, optimisation, Physiology, Scallops, Sensors, Shellfish, Shellfish culture, Statistical analysis, Strain gauges, Tracking, valvometry, Warning system, Warning systems},
