	author = {Lopes, Vanessa M and Lopes, Ana Rita and Costa, Pedro and Rosa, Rui},
	year = {2013},
	note = {Place: Basel
Publisher: MDPI AG},
	keywords = {cephalopods, Dosidicus gigas, harmful algal bloom, marine toxins, Octopus vulgaris, Pharmacy And Pharmacology, Sepia officinalis, strandings},
	pages = {3381--3409},
