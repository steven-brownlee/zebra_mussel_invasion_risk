@article{valkova_content_2022,
	title = {Content of {Pb} and {Zn} in {Sediments} and {Hydrobionts} as {Ecological} {Markers} for {Pollution} {Assessment} of {Freshwater} {Objects} in {Bulgaria}—{A} {Review}},
