	journal = {ZooKeys},
	author = {Wesselingh, Frank P and Neubauer, Thomas A and Anistratenko, Vitaliy V and Vinarski, Maxim V and Yanina, Tamara and {Jan Johan ter Poorten} and Kijashko, Pavel and Albrecht, Christian and {Olga Yu Anistratenko} and {Anouk D’Hont} and Frolov, Pavel and {Alberto Martínez Gándara} and Gittenberger, Arjan and Gogaladze, Aleksandre and Karpinsky, Mikhail and Lattuada, Matteo and Popa, Luis and Sands, Arthur F and van de Velde, Sabrina and Vandendorpe, Justine and Wilke, Thomas},
