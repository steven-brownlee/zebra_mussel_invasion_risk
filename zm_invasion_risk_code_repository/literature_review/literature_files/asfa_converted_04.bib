@article{kaloyianni_toxicity_2021,
	title = {Toxicity and {Functional} {Tissue} {Responses} of {Two} {Freshwater} {Fish} after {Exposure} to {Polystyrene} {Microplastics}},
	volume = {9},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/scholarly-journals/toxicity-functional-tissue-responses-two/docview/2602266999/se-2?accountid=13800},
	doi = {10.3390/toxics9110289},
	abstract = {Microplastics (MPs)’ ingestion has been demonstrated in several aquatic organisms. This process may facilitate the hydrophobic waterborne pollutants or chemical additives transfer to biota. In the present study the suitability of a battery of biomarkers on oxidative stress, physiology, tissue function and metabolic profile was investigated for the early detection of adverse effects of 21-day exposure to polystyrene microplastics (PS-MPs, sized 5–12 μm) in the liver and gills of zebrafish Danio rerio and perch, Perca fluviatilis, both of which are freshwater fish species. An optical volume map representation of the zebrafish gill by Raman spectroscopy depicted 5 μm diameter PS-MP dispersed in the gill tissue. Concentrations of PS-MPs close to the EC50 of each fish affected fish physiology in all tissues studied. Increased levels of biomarkers of oxidative damage in exposed fish in relation to controls were observed, as well as activation of apoptosis and autophagy processes. Malondialdehyde (MDA), protein carbonyls and DNA damage responses differed with regard to the sensitivity of each tissue of each fish. In the toxicity cascade gills seemed to be more liable to respond to PS-MPs than liver for the majority of the parameters measured. DNA damage was the most susceptible biomarker exhibiting greater response in the liver of both species. The interaction between MPs and cellular components provoked metabolic alterations in the tissues studied, affecting mainly amino acids, nitrogen and energy metabolism. Toxicity was species and tissue specific, with specific biomarkers responding differently in gills and in liver. The fish species that seemed to be more susceptible to MPs at the conditions studied, was P. fluviatilis compared to D. rerio. The current findings add to a holistic approach for the identification of small sized PS-MPs’ biological effects in fish, thus aiming to provide evidence regarding PS-MPs’ environmental impact on wild fish populations and food safety and adequacy.},
	language = {English},
	number = {11},
	journal = {Toxics},
	author = {Kaloyianni, Martha and Bobori, Dimitra C and Xanthopoulou, Despoina and Malioufa, Glykeria and Sampsonidis, Ioannis and Kalogiannis, Stavros and Feidantsis, Konstantinos and Kastrinaki, Georgia and Dimitriadi, Anastasia and Koumoundouros, George and Lambropoulou, Dimitra A and Kyzas, George Z and Bikiaris, Dimitrios N},
	year = {2021},
	note = {Place: Basel
Publisher: MDPI AG},
	keywords = {Additives, Adequacy, Amino acids, Animal tissues, Apoptosis, Aquatic organisms, Autophagy, Biological effects, Biology, Biomarker, Biomarkers, Biota, Carbonyl compounds, Carbonyls, Chelonia mydas, Crete, Damage, Danio rerio, Deoxyribonucleic acid--DNA, Diameters, DNA damage, DNA repair, Energy metabolism, Environmental impact, Exposure, Fish, Fish physiology, Fish populations, Food, Food safety, Fourier transforms, Freshwater fish, gills, Gills, Hydrophobicity, Ingestion, Japan, liver, Liver, Malondialdehyde, Metabolism, metabolomics, microplastics, Microplastics, Microscopy, Oxidative stress, oxidative stress biomarkers, Perca fluviatilis, Phagocytosis, Physiology, Plastic pollution, Pollutants, polystyrene, Polystyrene, Polystyrene resins, Raman spectroscopy, Salinity, Software, Species, Toxicity, Zebrafish},
