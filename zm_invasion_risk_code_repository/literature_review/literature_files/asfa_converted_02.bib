Publisher: Springer Nature B.V.},
	keywords = {Bacteria, Bioinformatics, Biology--Microbiology, Chemical compounds, Geographical distribution, Ions, Lead compounds, Libraries, Mass spectrometry, Mass spectroscopy, Metabolites, Microbiomes, Microorganisms, Pharmacology, Phylogenetics, Phylogeny, Populations, Proteins, Species diversity, Sponge, Sponges, Taxa, Taxonomy},
	pages = {22},
