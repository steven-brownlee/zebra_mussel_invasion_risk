@article{hensel_large_2021,
	title = {A large invasive consumer reduces coastal ecosystem resilience by disabling positive species interactions},
	volume = {12},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/scholarly-journals/large-invasive-consumer-reduces-coastal-ecosystem/docview/2590782151/se-2?accountid=13800},
	doi = {10.1038/s41467-021-26504-4},
	abstract = {Invasive consumers can cause extensive ecological damage to native communities but effects on ecosystem resilience are less understood. Here, we use drone surveys, manipulative experiments, and mathematical models to show how feral hogs reduce resilience in southeastern US salt marshes by dismantling an essential marsh cordgrass-ribbed mussel mutualism. Mussels usually double plant growth and enhance marsh resilience to extreme drought but, when hogs invade, switch from being essential for plant survival to a liability; hogs selectively forage in mussel-rich areas leading to a 50\% reduction in plant biomass and slower post-drought recovery rate. Hogs increase habitat fragmentation across landscapes by maintaining large, disturbed areas through trampling of cordgrass during targeted mussel consumption. Experiments and climate-disturbance recovery models show trampling alone slows marsh recovery by 3x while focused mussel predation creates marshes that may never recover from large-scale disturbances without hog eradication. Our work highlights that an invasive consumer can reshape ecosystems not just via competition and predation, but by disrupting key, positive species interactions that underlie resilience to climatic disturbances.Invasive species could have cascading effects on ecosystem functioning. Here the authors use experimental and remote sensing data and modelling to show that an invasive mammal causes loss of facilitative interactions between sessile ecosystem engineers in salt marshes, and lower ecosystem resilience to disturbance.},
	language = {English},
	number = {1},
	journal = {Nature Communications},
	author = {Hensel, S, Marc J and Silliman, Brian R and {van de Koppel Johan} and {Hensel Enie} and Sharp, Sean J and Crotty, Sinead M and {Byrnes Jarrett E K}},
	year = {2021},
	note = {Place: London
Publisher: Nature Publishing Group},
	keywords = {Climate models, Coastal ecosystems, Disturbance, Disturbances, Drought, Ecological effects, Ecosystem, Ecosystem resilience, Ecosystems, Eradication, Foraging habitats, Habitat fragmentation, Herbivores, Hogs, Introduced species, Invasive species, Liability, Marsh, Mathematical models, Mollusks, Mussel, Mussels, Mutualism, Plant biomass, Plant growth, Predation, Recovery, Remote sensing, Resilience, Salt marsh, Salt marshes, Sciences: Comprehensive Works, Spartina, Trampling},
	annote = {Copyright - © The Author(s) 2021. This work is published under http://creativecommons.org/licenses/by/4.0/ (the “License”). Notwithstanding the ProQuest Terms and Conditions, you may use this content in accordance with the terms of the License.},
	annote = {Last updated - 2023-11-30},
