	journal = {bioRxiv},
	author = {Stepien, Carol A and Elz, Anna E and Snyder, Matthew R and {Genetics And Genomics Group}},
	year = {2018},
	note = {Place: Cold Spring Harbor},
	keywords = {Aquaculture, Asia, Biology, Colonization, Cytochrome, Cytochrome b, Cytochrome-c oxidase, Environmental DNA, Fish, Food availability, Genetic diversity, Haplotypes, Lakes, Microsatellites, Mississippi River, Mitochondria, Mitochondrial DNA, Nucleotide sequence, Plankton, Population, Population differentiation, Population genetics, Population studies, Ribosomal protein S7, River basins, Rivers, Wabash River},
