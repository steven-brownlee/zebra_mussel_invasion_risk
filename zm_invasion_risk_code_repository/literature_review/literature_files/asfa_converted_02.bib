Publisher: Regional Euro-Asian Biological Invasions Centre},
	keywords = {Algae, Biology, Dispersal, Great Lakes, Introduced species, Invasive species, Lakes, Mollusca, Nonnative species, Risk assessment, Surveillance, Taxonomy},
	pages = {272--272--293},
