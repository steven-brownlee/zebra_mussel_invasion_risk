}

@mastersthesis{hamilton_expansion_2021,
	address = {Canada -- Saskatchewan, CA},
	title = {Expansion of {Invasive} {Prussian} {Carp} ({Carassius} {Gibelio}) into {Saskatchewan}, {Canada}: {Current} {Distribution} and {Food}-{Web} {Reconstruction} {Using} {Stable} {Isotopes}},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/dissertations-theses/expansion-invasive-prussian-carp-i-carassius/docview/2652593343/se-2?accountid=13800},
	abstract = {The Prussian carp (Carassius gibelio) is one of the most invasive fish species inEurope and was recently introduced into North America. This species was introducedinto Alberta waters in the early 2000s and its range has subsequently expanded. In 2018an established population of Prussian carp was discovered in Saskatchewan for the firsttime. In response to this discovery, I undertook the first study of this invasive fishspecies in Saskatchewan. I used a combination of field studies and citizen sciencereporting to conduct surveys for Prussian carp in Saskatchewan. I also used stableisotopes analysis of Prussian carp in Stockwell Lake to perform the first diet analysis forPrussian carp in North America. I used Carbon, Nitrogen, and Sulfur isotopes todetermine the diet composition based on prey items sampled. Prussian carp were foundthroughout the South Saskatchewan River system from the Alberta border to north ofSaskatoon. Citizen science reports were a much more effective way of surveying forPrussian carp than focused netting in specific areas. Bayesian mixing models showedthat Prussian carp diet in Stockwell lake consisted primarily of chironomids andzooplankton, and that diet composition shifted as Prussian carp increased in size. Largerfish used more benthic food resources (e.g., chironomids) than smaller ones. My workshows that Prussian carp are now well established in Saskatchewan and have thepotential to continue expanding their range into Manitoba and the United States. Dietanalyses confirmed that the Prussian carp is a generalist omnivore in Saskatchewan, andsuggests that this species may be very flexible in how it uses prey resources. Ultimately,the Prussian carp is an aquatic invasive species that has the potential to be verydamaging in Saskatchewan and will use provincial waterbodies as a conduit to colonizemore areas.},
	language = {English},
	school = {The University of Regina (Canada)},
	author = {Hamilton, Shayna Nicole},
	collaborator = {Somers, Christopher and Balshine, Sigal},
	year = {2021},
	note = {ISBN: 979-8-209-91939-1
Publication Title: ProQuest Dissertations and Theses
29020222},
	keywords = {0385:Canadian studies, Canadian studies, Carp, Creeks \& streams, Food chains, Rivers, Watersheds},
