	note = {Place: Ottawa
Publisher: Canadian Science Publishing NRC Research Press},
	keywords = {Animal reproduction, Biology--Zoology, Dreissena, Life cycles, Mortality, Mussel, Zebra mussel, Zoology},
	pages = {1436--1443},
	annote = {CODEN - CJZOAG},
