	note = {Place: Dordrecht
Publisher: Springer Nature B.V.},
	keywords = {1530:Natural resources, 9190:United States, Adaptive management, Biodiversity, Biophysical environment, Birds, Costs, Economic impact, Endangered \& extinct species, Endangered species, Environmental degradation, Environmental impact, Invasive species, Mollusks, Native species, Nonnative species, Pathogens, Philosophy, Political opportunity, Politics, United States--US},
	pages = {81--88},
