	annote = {Last updated - 2023-11-25},
	annote = {SubjectsTermNotLitGenreText - Puerto Rico; United States--US},
}

@article{yan_genetic_2023,
	title = {Genetic {Analysis} {Based} on {Mitochondrial} nad2 {Gene} {Reveals} a {Recent} {Population} {Expansion} of the {Invasive} {Mussel}, {Mytella} strigata, in {China}},
	volume = {14},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/scholarly-journals/genetic-analysis-based-on-mitochondrial-i-nad2/docview/2893046337/se-2?accountid=13800},
	doi = {10.3390/genes14112038},
	abstract = {Mytella strigata is a highly adaptable invasive alien species that has been established in coastal China since 2014. Mitochondrial DNA (mtDNA) is an important tool for studying the evolution and population genetics of invasive species. In this study, the mitochondrial genome of M. strigata from China was sequenced by Illumina high-throughput sequencing and characterized with 13 protein-coding genes (PCGs). By assessing the selective pressure of 13 PCGs, the nad2 gene had the fastest evolutionary rate and was finally selected for population genetic analysis. A total of 285 nad2 sequences from seven M. strigata populations in China were analyzed and showed obviously T-rich and C-rich characteristics. According to population genetic diversity analysis, all the seven populations had haplotype (gene) diversity (Hd) ≥ 0.5 and nucleotide diversity (Pi) {\textless} 0.005. Haplotype networks showed a “star” distribution. Population historical dynamic analyses showed that Fu’s Fs and Tajima’s D values of all populations were negative except the Qukou (QK) and Beihai (BH) populations. The Zhangzhou (ZJ) and Xiamen (XM) populations were unimodal while the other populations were multimodal. These results suggested that the population of M. strigata in China may have passed the bottleneck period and is currently in a state of population expansion.},
	language = {English},
	number = {11},
	journal = {Genes},
	author = {Yan, Shaojing and Ma, Peizhen and Zuo, Chenxia and Zhu, Yi and Ma, Xiaojie and Zhang, Zhen},
	year = {2023},
	note = {Place: Basel
Publisher: MDPI AG},
	keywords = {alien species, Beijing China, biological invasion, Biology--Genetics, Biotechnology, China, Evolution, Genetic analysis, Genetic diversity, Genomes, Haplotypes, Introduced species, Invasive species, Mitochondrial DNA, Mutation, Mytella strigata, nad2, Next-generation sequencing, Nonnative species, population genetics, Population genetics, Shipping industry, Software},
