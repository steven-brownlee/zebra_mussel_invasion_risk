@article{feiner_species_2018,
	title = {Species identity matters when interpreting trophic markers in aquatic food webs},
	volume = {13},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/scholarly-journals/species-identity-matters-when-interpreting/docview/2116607632/se-2?accountid=13800},
	doi = {10.1371/journal.pone.0204767},
	abstract = {In aquatic systems, food web linkages are often assessed using diet contents, stable isotope ratios, and, increasingly, fatty acid composition of organisms. Some correlations between different trophic metrics are assumed to be well-supported; for example, particular stable isotope ratios and fatty acids seem to reflect reliance on benthic or pelagic energy pathways. However, understanding whether the assumed correlations between different trophic metrics are coherent and consistent across species represents a key step toward their effective use in food web studies. To assess links among trophic markers, we compared relationships between major diet components, fatty acids, and stable isotope ratios in three fishes: yellow perch (Perca flavescens), round goby (Neogobius melanostomus), and spottail shiner (Notropis hudsonius) collected from nearshore Lake Michigan. Yellow perch and spottail shiner are native in this system, while round goby are a relatively recent invader. We found some evidence for agreement between different trophic metrics, especially between diet components, n-3:n-6 fatty acid ratios, and stable isotope ratios (δ13C and δ15N). However, we also observed significant variation in observed relationships among markers and species, potentially due to taxonomic variation in the specific diet items consumed (e.g., chydorid microcrustaceans and Dreissena mussels) and species-specific biochemical processes. In many of these latter cases, the invasive species differed from the native species. Understanding the effects of taxonomic variation on prey and predator signatures could significantly improve the usefulness of fatty acids in food web studies, whereas diet contents and stable isotopes appear to be reliable indicators of trophic niche in aquatic food webs.},
	language = {English},
	number = {10},
	journal = {PLoS One},
	author = {Feiner, Zachary S and {⨯ Carolyn J Foley} and {⨯ Harvey A Bootsma} and Czesny, Sergiusz J and Janssen, John and Rinchard, Jacques and Höök, Tomas O},
