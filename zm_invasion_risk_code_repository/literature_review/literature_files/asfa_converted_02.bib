@article{medrzycki_simple_2017,
	title = {Simple yet effective: {Historical} proximity variables improve the species distribution models for invasive giant hogweed ({Heracleum} mantegazzianum s.l.) in {Poland}},
	volume = {12},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/scholarly-journals/simple-yet-effective-historical-proximity/docview/1940529739/se-2?accountid=13800},
	doi = {10.1371/journal.pone.0184677},
	abstract = {Species distribution models are scarcely applicable to invasive species because of their breaking of the models’ assumptions. So far, few mechanistic, semi-mechanistic or statistical solutions like dispersal constraints or propagule limitation have been applied. We evaluated a novel quasi-semi-mechanistic approach for regional scale models, using historical proximity variables (HPV) representing a state of the population in a given moment in the past. Our aim was to test the effects of addition of HPV sets of different minimal recentness, information capacity and the total number of variables on the quality of the species distribution model for Heracleum mantegazzianum on 116000 km2 in Poland. As environmental predictors, we used fragments of 103 1×1 km, world- wide, free-access rasters from WorldGrids.org. Single and ensemble models were computed using BIOMOD2 package 3.1.47 working in R environment 3.1.0. The addition of HPV improved the quality of single and ensemble models from poor to good and excellent. The quality was the highest for the variants with HPVs based on the distance from the most recent past occurrences. It was mostly affected by the algorithm type, but all HPV traits (minimal recentness, information capacity, model type or the number of the time periods) were significantly important determinants. The addition of HPVs improved the quality of current projections, raising the occurrence probability in regions where the species had occurred before. We conclude that HPV addition enables semi-realistic estimation of the rate of spread and can be applied to the short-term forecasting of invasive or declining species, which also break equal-dispersal probability assumptions.},
	language = {English},
	number = {9},
	journal = {PLoS One},
	author = {Mędrzycki, Piotr and Jarzyna, Ingeborga and Obidziński, Artur and Tokarska-Guzik, Barbara and Sotek, Zofia and Pabjanek, Piotr and Pytlarczyk, Adam and Sachajdakiewicz, Izabela},
	month = sep,
	year = {2017},
	note = {Place: San Francisco
Publisher: Public Library of Science},
	keywords = {Algorithms, Biology, Botany, Breaking, Census, Dispersal, Dispersion, Environmental geography, Environmental protection, Equilibrium, Forecasting, Fragments, Heracleum mantegazzianum, Introduced species, Invasive species, Laboratories, Land use, Nonnative species, Plant ecology, Poland, Population (statistical), Population decline, Regional analysis, Regional-scale models, Scale models, Sciences: Comprehensive Works, Specie, Species distribution, Statistical analysis, Statistical distributions, Trends, Variables},
