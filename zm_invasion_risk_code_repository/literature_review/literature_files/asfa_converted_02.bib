@article{heinrich_olives_2021,
	title = {Of olives and carp: interactive effects of an aquatic and a terrestrial invader on a stream‐riparian ecosystem},
	volume = {12},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/scholarly-journals/olives-carp-interactive-effects-aquatic/docview/2586587536/se-2?accountid=13800},
	doi = {10.1002/ecs2.3789},
	abstract = {Multiple invasive species may interact, influencing one another and generating synergistic effects on food webs and ecosystem processes. We investigated the interaction between two non‐native species widespread in the western USA: common carp (Cyprinus carpio) and Russian olive (Elaeagnus angustifolia), an invasive riparian tree associated with di‐nitrogen fixation. Deep Creek, Idaho, was an International Biological Program site in the early 1970s; at that time, carp were rare and Russian olive was absent. Subsequently, Russian olive was introduced and established a dense stand, increasing allochthonous inputs and nitrogen‐rich benthic organic matter. Since 1971, carp density has increased {\textasciitilde}4× (an increase our bioenergetic analysis suggests could not have been sustained in the absence of Russian olive). Carp gut contents in 2013–2014 revealed, on average, {\textasciitilde}40\% olives, and, similarly, stable isotope analyses revealed {\textasciitilde}58\% of carp tissues were derived from olives. A small‐scale, short‐term experimental exclusion of these subsidized carp caused {\textasciitilde}3× increases in macrophytes and chlorophyll‐a, suggesting they may limit algae and macrophyte biomass. Moreover, carp that consumed olives excreted more nitrogen ({\textasciitilde}2× more ammonium, {\textasciitilde}2× more total dissolved nitrogen, and {\textasciitilde}3× more total nitrogen) compared to those that had not, which may amplify recycling and export from streams invaded by both species. This scenario is characteristic of an “invasional meltdown,” with attendant changes in food webs and ecosystem processes.},
	language = {English},
	number = {10},
	journal = {Ecosphere},
	author = {Heinrich, Kaleb K and Baxter, Colden V and Bell, Alex T C and Hood, James M},
