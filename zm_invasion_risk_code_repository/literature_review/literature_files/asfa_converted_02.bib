@article{micael_bryozoa_2016,
	title = {Some {Bryozoa} species recently introduced into the {Azores}: reproductive strategies as a proxy for further spread},
	volume = {70},
	issn = {1438387X},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/scholarly-journals/some-bryozoa-species-recently-introduced-into/docview/2575269632/se-2?accountid=13800},
	doi = {10.1186/s10152-016-0458-7},
	abstract = {In the marine environment, control of invasive species’ population levels, that is, keeping them at an abundance level which is below a density-dependent adverse effect, may be the most attainable goal for the management of introduced bryozoans. An improved understanding of reproductive strategies and life history traits is key in order to understand the spreading potential. The assessment of the magnitude and temporal dynamics of propagule pressure from the reproducing population important for the success of control actions and needs is to be determined prior to any field intervention. The reproductive cycle of three fouling bryozoans (Bugula neritina, Tricellaria inopinata and Virididentula dentata) in the waters of the Azores Archipelago was assessed. The study revealed that although the release of larvae can occur throughout the entire year, its intensity and developmental and the attachment success of the ancestrula are not even throughout the year and that each species’ reproductive development needs to be determine independently. In the light of these findings, it is possible to determine the best time to apply field actions aimed at controlling invasive population’s density levels, optimizing the always scarce financial resources for marine management.},
	language = {English},
	journal = {Helgoland Marine Research},
