	author = {Edwards, William J and Rehmann, Chris R and McDonald, Ellen and Culver, David A},
	month = jan,
	year = {2005},
	note = {Place: Ottawa
Publisher: Canadian Science Publishing NRC Research Press},
	keywords = {Algae, Benthos, Earth Sciences, Fluid dynamics, Impact analysis, Lake Erie, Marine ecology, Plankton, Shorelines, Zebra mussel},
	pages = {205--214},
	annote = {CODEN - CJFSDX},
