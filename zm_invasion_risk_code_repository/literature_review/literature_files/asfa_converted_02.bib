Publisher: MDPI AG},
	keywords = {Accuracy, Algorithms, Animals, Automation, Behavior, Bias, biohybrid, Biophysical environment, biosensor, Biosensors, Cameras, Chemistry--Analytical Chemistry, Daphnia, Environment models, environmental monitoring, Environmental monitoring, False alarms, False negatives, judgment and decision making, Organisms, Outdoor air quality, Sensors, signal detection, sustainable environmental monitoring technology, Water quality},
