@article{uliano-silva_gene_2014,
	title = {Gene {Discovery} through {Transcriptome} {Sequencing} for the {Invasive} {Mussel} {Limnoperna} fortunei},
	volume = {9},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/scholarly-journals/gene-discovery-through-transcriptome-sequencing/docview/1547308564/se-2?accountid=13800},
	doi = {10.1371/journal.pone.0102973},
	abstract = {The success of the Asian bivalve Limnoperna fortunei as an invader in South America is related to its high acclimation capability. It can inhabit waters with a wide range of temperatures and salinity and handle long-term periods of air exposure. We describe the transcriptome of L. fortunei aiming to give a first insight into the phenotypic plasticity that allows non-native taxa to become established and widespread. We sequenced 95,219 reads from five main tissues of the mussel L. fortunei using Roche’s 454 and assembled them to form a set of 84,063 unigenes (contigs and singletons) representing partial or complete gene sequences. We annotated 24,816 unigenes using a BLAST sequence similarity search against a NCBI nr database. Unigenes were divided into 20 eggNOG functional categories and 292 KEGG metabolic pathways. From the total unigenes, 1,351 represented putative full-length genes of which 73.2\% were functionally annotated. We described the first partial and complete gene sequences in order to start understanding bivalve invasiveness. An expansion of the hsp70 gene family, seen also in other bivalves, is present in L. fortunei and could be involved in its adaptation to extreme environments, e.g. during intertidal periods. The presence of toll-like receptors gives a first insight into an immune system that could be more complex than previously assumed and may be involved in the prevention of disease and extinction when population densities are high. Finally, the apparent lack of special adaptations to extremely low O2 levels is a target worth pursuing for the development of a molecular control approach.},
	language = {English},
	number = {7},
	journal = {PLoS One},
	author = {Uliano-Silva, Marcela and {Juliana Alves Americo} and Brindeiro, Rodrigo and Dondero, Francesco and Prosdocimi, Francisco and {Mauro de Freitas Rebelo}},
	month = jul,
	year = {2014},
	note = {Place: San Francisco
Publisher: Public Library of Science},
	keywords = {Acclimation, Acclimatization, Adaptation, Air exposure, Algorithms, Biophysics, Bivalves, Bivalvia, BLAST algorithm, Brazil, Databases, Ecosystems, Environmental impact, Extreme environments, Gene, Gene expression, Gene sequencing, Genomes, Genomics, Hsp70 protein, Immune system, Invasiveness, Limnoperna fortunei, Metabolic pathways, Mollusks, Mussel, Mussels, Phenotypic plasticity, Phylogenetic analysis, Proteins, Receptors, Rio de Janeiro Brazil, Sciences: Comprehensive Works, Sequence databases, Sequence motif analysis, Sequence similarity searching, South America, Sugarcane, Temperature range, Tissues, Toll-like receptors, Transcriptome, Transcriptome analysis},
