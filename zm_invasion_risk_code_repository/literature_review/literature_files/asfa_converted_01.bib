Publisher: John Wiley \& Sons, Inc.},
	keywords = {12S, Biology--Genetics, Climate change, COI, Conservation, DNA barcoding, eDNA metabarcoding, Endangered species, Environmental DNA, Fish, Freshwater fish, Grand River, Great Lakes of North America, Habitats, Indigenous species, Lake Michigan, Lakes, Michigan, Mollusks, Mussels, Outlets, River networks, Rivers, species co-distribution, Taxa, Threatened species, Tributaries, unionid mussels, United States--US, Watersheds},
	pages = {424--437},
