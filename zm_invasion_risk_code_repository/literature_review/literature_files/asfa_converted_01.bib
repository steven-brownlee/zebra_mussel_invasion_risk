	note = {Publication Title: Non-Native Species and Their Role in the Environment},
	keywords = {Biological diversity, Conservation biology, Introduced organisms--Environmental aspects., Introduced organisms-Environmental aspects., Life sciences, Native species, Nonnative species, RNCB, SCI088000:SCIENCE / Life Sciences / Biological Diversity, Science},
	annote = {Copyright - © 2016 BRILL},
