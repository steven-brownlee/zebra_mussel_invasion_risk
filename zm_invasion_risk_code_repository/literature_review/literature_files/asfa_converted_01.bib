@mastersthesis{sugimoto_comparing_2022,
	address = {United States -- California},
	title = {Comparing {Contaminant}-{Induced} {Gene} {Expression} in {Native} and {Non}-{Native} {Oysters} in {Southern} {California} {Estuaries}},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/dissertations-theses/comparing-contaminant-induced-gene-expression/docview/2723177872/se-2?accountid=13800},
	abstract = {The estuarine ecosystems of Southern California serve numerous ecological and commercial functions but are under increasing anthropogenic pressure. Pollution and the expansion of nonindigenous species challenge these already vulnerable locales and the native species that inhabit them. The purpose of this study was to quantify the different types and concentrations of chemical contaminants detected in oyster tissue and surrounding sediment from Southern California estuaries. Additionally, transcriptomic analysis was utilized to investigate differential gene expression to compare site-specific and species-specific stress responses between a native and non-native oyster species. Tissue samples of the native Ostrea lurida and the non-native Crassostrea gigas as well as sediment samples were collected from six different sites across four estuaries. Tissue and sediment samples underwent chemical contaminant analysis to quantify concentrations of trace metal and organic contaminants while gill tissue samples underwent RNA-sequencing for differential gene expression analysis. Concentrations of various trace metal and organic analytes exceeded screening levels at multiple sites with sediment and C. gigas tissue varying statistically in their contaminant levels across sites. Contaminant types and concentrations reported here align with findings of past studies, indicating that various contaminants remain throughout Southern California estuaries in both biotic and environmental compartments. Notably, tissue and sediment samples from the San Diego Bay Boathouse (SDBBH) site displayed qualitatively the highest concentrations of PCBs, highlighting the need for further monitoring and assessment of legacy contaminants in estuarine habitats. Transcriptomic analysis revealed altered expression of various genes involved in oxidative stress and xenobiotic biotransformation. Specifically, we observed increased expression of glutathione-S-transferase in both O. lurida and C. gigas from SDBBH, which suggests increased PCB concentrations may be impacting the health of both oysters. Differential gene expression analysis between species was evaluated based on patterns of differentially expressed genes (DEGs) and gene ontology (GO) terms. Overall, no GO terms were shared between the two species which provides evidence for a species-specific stress response. C. gigas exhibited enrichment of GO terms involved in ATP production and transporter pathways while O. lurida exhibited changes in GO terms involved in cellular homeostasis. These findings demonstrate that oysters throughout Southern California estuaries with varying contaminant levels displayed differential gene expression and a species-specific response observed in C. gigas may confer a higher tolerance to stressful conditions.},
	language = {English},
	school = {California State University, Long Beach},
	author = {Sugimoto, Aaron K.},
	collaborator = {Holland, Erika B.},
	year = {2022},
	note = {ISBN: 9798352906217
Publication Title: ProQuest Dissertations and Theses
29066169},
	keywords = {0306:Biology, Biology, Contaminants, Estuaries, Non-native, Oysters, Stress response, Transcriptomics},
	annote = {Copyright - Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
	annote = {Last updated - 2023-07-07},
}

@article{miess_ecological_2022,
	title = {An {Ecological} {Profile} of {Hydropsyche} alternans ({Trichoptera}: {Hydropsychidae}) in {Lake} {Superior}, the {Last} {Stronghold} of a {Once}-{Dominant} {Great} {Lakes} {Surf} {Zone} {Caddisfly}},
	volume = {13},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/scholarly-journals/ecological-profile-i-hydropsyche-alternans/docview/2694010981/se-2?accountid=13800},
	doi = {10.3390/insects13070659},
	abstract = {Simple SummaryPrior to the invasion and spread of Dreissena mussels in the late 1980s, the nearshore waters of the Laurentian Great Lakes were home to diverse assemblages of native aquatic insects, including the net-spinning caddisfly Hydropsyche alternans, which occurred in rocky surf-exposed habitat throughout the system from Lake Superior downstream to Lake Ontario. These surf zone caddisflies are still abundant in the largely Dreissena-free waters of Lake Superior where the present study was conducted. They have not been reported in the lakes below Lake Superior for decades, and are presumed to have been extirpated from Dreissena-infested habitats. The ecological profile presented here documents the life history of H. alternans in Lake Superior, and reveals details of its feeding biology that shed light on the roles that surf zone net-spinners play in the native nearshore food webs of the Great Lakes. The H. alternans life history in Lake Superior begins in mid-summer shortly after oviposition, which features females swimming to surf-exposed lake bottom substrates. Larval development takes approximately 10 months, but most of it occurs during their first 100 days. Adult emergence is broadly synchronous, with large numbers present from the summer solstice through mid-July. Gut content analyses showed that larvae opportunistically feed on algal, animal, and detrital material of aquatic and terrestrial origin. δ13C and δ15N stable isotope data indicate that they function as omnivores that link coastal, nearshore, and pelagic food webs. These energetic links, and the very existence of surf zone insect assemblages in the Great Lakes, depend on the Dreissena populations in Lake Superior remaining relatively small and isolated.AbstractWe studied the life history, diet, and trophic ecology of Hydropsyche alternans in four rocky sites located along the south-central coast of Lake Superior. The H. alternans life history and broad trophic niche space were similar to those of its riverine relatives. Quantitative sampling over the course of one ice-free season revealed that most individuals lived univoltine life histories that featured early to mid-summer mating, and oviposition and rapid growth and development through summer into fall. Most individuals overwintered as ultimate or penultimate larval instars. Pupation followed ice-out in the spring. Gut content sampling and δ13C and δ15N stable isotope analyses indicated that the typical larval diet is a mix of benthic, pelagic, and terrestrial food resources, including diatoms, small arthropods, sloughed periphyton, and in one site, fugal hyphae apparently of foredune origin. As a suspension-feeding omnivore that relies on waves and currents to deliver food to its nets, H. alternans larvae form energetic links between coastal, nearshore, and offshore food webs. These connections have been lost throughout the lower Laurentian Great Lakes as a consequence of the invasion and spread of Dreissena mussels.},
	language = {English},
	number = {7},
	journal = {Insects},
	author = {Miess, Sam and Chrisekos, Alissa and Strand, Mac},
	year = {2022},
	note = {Place: Basel
Publisher: MDPI AG},
	keywords = {Algae, Alternan, Aquatic insect, Aquatic insects, Arthropods, autecology, Biology--Entomology, Caddisfly, Creeks \& streams, Developmental stages, Diet, Dreissena, Energy, Exposed habitats, Food, Food chains, Food resources, Food webs, Hydropsyche, Hydropsychidae, Hyphae, Ice sheets, Insects, Instars, Lake Agassiz, Lake Michigan, Lake Superior, Lake Winnipeg, Lakes, Larvae, Larval development, Life history, Marine microorganisms, Michigan, Mollusks, Mussels, Omnivores, Oviposition, Periphyton, Pupation, Rivers, Sampling, Shorelines, Spinners, Stable isotope ratio, Stable isotopes, Substrates, Summer, Surf, surf zone, Surf zone, Swimming, Taxonomy, Terrestrial environments, Trichoptera, trophic ecology, United States--US, Water quality, Wave power},
	pages = {659},
