@article{martinson_aquatic_2014,
	address = {Rocky Mountain House, Alta.},
	title = {Aquatic invasive species are a serious threat to {Canadian} water bodies},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/newspapers/aquatic-invasive-species-are-serious-threat/docview/1550461682/se-2?accountid=13800},
	abstract = {Easily spread from water body to water body, species like the zebra mussel (Dreissena polymorpha), quagga mussel (Dreissena bugensis) and Eurasian water milfoil (Myriophyllum spicatum) can hitch a ride on ATVs, boats and equipment used in water sports like fishing gear, life jackets, footwear and paddles.


While the aquatic plant can offer a food source and habitable location for some fish and invertebrates, Eurasian water milfoil is quick to become problematic. Rapid growth results in thick floating mats of the plant that prohibit light from penetrating the water body. Dissolved oxygen levels are reduced as the Eurasian water milfoil decomposes. These circumstances limit the growth of native beneficial aquatic plants and animals, reducing biodiversity.


Eurasian water milfoil, quagga mussels, and zebra mussels are just three examples of invasive species to watch for in our province. Species such as the New Zealand mudsnail (Potamopyrgus antipodarum), Asian carp, didymo (Didymosphenia geminate) aka Rock Snot and Salt Cedar (Tamarix spp.) are additional examples on the growing list of aquatic invaders.},
	language = {English},
	journal = {The Mountaineer},
	author = {Martinson, Matt},
	month = jul,
	year = {2014},
	keywords = {General Interest Periodicals--Canada},
	annote = {Copyright - Copyright Alberta Weekly Newspaper Association Jul 15, 2014},
	annote = {Last updated - 2014-08-02},
}

