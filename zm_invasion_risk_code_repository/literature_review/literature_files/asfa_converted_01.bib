@article{moffitt_toxicity_2016,
	title = {Toxicity of potassium chloride to veliger and byssal stage dreissenid mussels related to water quality},
	volume = {7},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/scholarly-journals/toxicity-potassium-chloride-veliger-byssal-stage/docview/2287043417/se-2?accountid=13800},
	abstract = {Natural resource managers are seeking appropriate chemical eradication and control protocols for infestations of zebra mussels, Dreissena polymorpha (Pallas, 1769), and quagga mussels. D. rostiformis bugensis (Andrusov, 1897) that have limited effect on non-target species. Applications of low concentrations of potassium salt (as potash) have shown promise for use where the infestation and treatment can be contained or isolated. To further our understanding of such applications and obtain data that could support a pesticide registration, we conducted studies of the acute and chronic toxicity of potassium chloride to dreissenid mussels in four different water sources from infested and non-infested locations (ground water from northern Idaho, surface water from the Snake River, Idaho, USA, surface water from Lake Ontario, Ontario, Canada, and surface water from the Colorado River, Arizona, USA). We found short term exposure of veligers ({\textless} 24 h) to concentrations of 960 mg/L KCl produced rapid mortality in water from three locations, but veligers tested in Colorado River water were resistant. We used probit models to compare the mortality responses, predicted median lethal times and 95\% confidence intervals. In separate experiments, we explored the sensitivity of byssal stage mussels in chronic exposures ({\textgreater}29 d) at concentrations of 100 and 200 mg/L KCl. Rapid mortality occurred within 10 d of exposure to concentrations of 200 mg/L KCl, regardless of water source. Kaplan-Meier estimates of mean survival of byssal mussels in 100 mg/L KCl prepared in surface water from Idaho and Lake Ontario were 4.9 or 6.9 d, respectively; however, mean survival of mussels tested in the Colorado River water was {\textgreater} 23 d. The sodium content of the Colorado River water was nearly three times that measured in waters from the other locations, and we hypothesized sodium concentrations may affect mussel survival. To test our hypothesis, we supplemented Snake River and Lake Ontario water with NaCl to equivalent conductivity as the Colorado River, and found mussel survival increased to levels observed in tests of veliger and byssal mussels in Colorado River water. We recommend KCl disinfection and eradication protocols must be developed to carefully consider the water quality characteristics of treatment locations.},
	language = {English},
	number = {3},
	journal = {Management of Biological Invasions},
	author = {Moffitt, Christine M and Stockton-Fiti, Kelly A and Claudi, Renata},
	month = sep,
	year = {2016},
	note = {Place: Almería
Publisher: Regional Euro-Asian Biological Invasions Centre},
	keywords = {Biology, Chronic toxicity, Colorado River, Disinfection, Groundwater, Idaho, Lake Ontario, Mollusks, Mortality, Potassium chloride, Rivers, Snake River, Sodium chloride, Surface water, Survival, United States--US, Water quality},
	pages = {257--268},
	annote = {Copyright - © 2016. Notwithstanding the ProQuest Terms and Conditions, you may use this content in accordance with the associated terms available at https://www.reabic.net/journals/mbi/About.aspx},
	annote = {Last updated - 2019-09-13},
	annote = {SubjectsTermNotLitGenreText - United States--US; Lake Ontario; Idaho; Snake River; Colorado River},
}

@book{guiasu_non-native_2016,
	title = {Non-{Native} {Species} and {Their} {Role} in the {Environment}: {The} {Need} for a {Broader} {Perspective}},
	isbn = {978-90-474-2613-4},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/books/non-native-species-their-role-environment/docview/2133268536/se-2?accountid=13800},
	abstract = {The young field of invasion biology - initially a branch of ecology and conservation biology - has greatly expanded, particularly in the last two and a half decades or so. As a result, the potential negative effects of introduced species have been widely advertised and sometimes, perhaps, overemphasized. This book attempts to restore some balance to the current debate over the role of non-native species, by offering a broader perspective, and taking a longer term, evolutionary look at these species and their impact in their new environments. The relatively arbitrary nature of terms such as "native" and "non-native", and the rather inconsistent ways in which such terms are applied to biological species, as well as the subjective boundaries of so-called "native ranges" are analyzed. The role of non-native species in their new environments can be considerably more complex than the anti-introduced species information would often suggest. Thus, the more positive and nuanced perspective on introduced species and their impact offered in this book is much needed and long overdue.},
	language = {English},
	publisher = {BRILL, BRILL},
	author = {Guiaşu, Radu Cornel},
	month = aug,
