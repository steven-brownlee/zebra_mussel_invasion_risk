	journal = {bioRxiv},
	author = {Mccartney, Michael A and Auch, Benjamin and Kono, Thomas and Mallez, Sophie and Zhang, Ying and {Angelico Obille} and Becker, Aaron and Abrahante, Juan E and Garbe, John and Badalamenti, Jonathan P and Herman, Adam and Mangelson, Hayley and Liachko, Ivan and Sullivan, Shawn and Sone, Eli D and Koren, Sergey and {Kevin At Silverstein} and Beckman, Kenneth B and Gohl, Daryl M},
	year = {2019},
	note = {Place: Cold Spring Harbor},
	keywords = {Aquatic ecosystems, Biology, Dreissena polymorpha, Genomes, Introduced species, Invasive species, Invasiveness, Mitochondria, Mollusks, Nonnative species, Temperature tolerance},
