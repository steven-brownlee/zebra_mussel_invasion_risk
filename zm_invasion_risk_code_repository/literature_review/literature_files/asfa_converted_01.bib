	note = {Place: Bristol
Publisher: IOP Publishing},
	keywords = {Aquatic ecosystems, Dispersal, Dreissena rostriformis bugensis, Environmental changes, Environmental Studies, Eutrophication, freshwater, future, lakes, Lakes, Mollusks, mussels, Mussels, neobiota, Northern Hemisphere, Particulates, Populations, quagga, Tolerances, Water circulation, Water column},
