@article{bunnell_impact_2005,
	title = {The impact of introduced round gobies ({Neogobius} melanostomus) on phosphorus cycling in central {Lake} {Erie}},
	volume = {62},
	issn = {0706652X},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/scholarly-journals/impact-introduced-round-gobies-neogobius/docview/219314860/se-2?accountid=13800},
	abstract = {We used an individual-based bioenergetic model to simulate the phosphorus flux of the round goby (Neogobius melanostomus) population in central Lake Erie during 1995-2002. Estimates of round goby diet composition, growth rates, and population abundance were derived from field sampling. As an abundant introduced fish, we predicted that round gobies would influence phosphorus cycling both directly, through excretion, and indirectly, through consumption of dreissenid mussels, whose high mass-specific phosphorus excretion enhances recycling. In 1999, when age-1+ round gobies reached peak abundance near 350 million (2.4 kg.ha{\textasciicircum}sup -1{\textasciicircum}), annual phosphorus excretion was estimated at 7 t (1.4 × 10{\textasciicircum}sup -3{\textasciicircum} mg P.m{\textasciicircum}sup -2{\textasciicircum}.day{\textasciicircum}sup -1{\textasciicircum}). From an ecosystem perspective, however, round gobies excreted only 0.4\% of the phosphorus needed by the benthic community for primary production. Indirectly, round gobies consumed {\textless}0.2\% of dreissenid population biomass, indicating that round gobies did not reduce nutrient availability by consuming dreissenids. Compared with previous studies that have revealed introduced species to influence phosphorus cycling, round gobies likely did not attain a sufficiently high biomass density to influence phosphorus cycling in Lake Erie. [PUBLICATION ABSTRACT]},
	language = {English},
	number = {1},
	journal = {Canadian Journal of Fisheries and Aquatic Sciences},
	author = {Bunnell, David B and Johnson, Timothy B and Knight, Carey T},
	month = jan,
	year = {2005},
	note = {Place: Ottawa
Publisher: Canadian Science Publishing NRC Research Press},
