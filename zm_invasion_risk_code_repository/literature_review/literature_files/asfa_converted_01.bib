Publisher: MDPI AG},
	keywords = {biomineralization, Biosynthesis, Chemistry--Organic Chemistry, China, Evolution, fibrillin gene family, Genes, genome, Genomes, Genomics, Immune system, Mollusks, pearl, Pearls, Phylogenetics, Proteins, Sinohyriopsis cumingii},
