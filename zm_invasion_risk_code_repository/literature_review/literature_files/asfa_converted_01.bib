Publisher: MDPI AG},
	keywords = {Abnormalities, Acute toxicity, Aquatic organisms, Bangladesh, Bioaccumulation, Biology, Chronic toxicity, Condition factor, Control equipment, Food, Heavy metals, Hemocytes, Histology, histopathology, Hydrology, Immunology, Kidneys, Laboratories, Lamellidens marginalis, LC50, Lead, Low level, Metals, Mollusks, Muscles, Parameters, Pollutants, Pollution, Sediments, serum lysosomal activity, Soft tissues, specific growth rate, Survival, Tissues, Toxicity, Toxicity testing, United States--US, Weight},
	pages = {475},
