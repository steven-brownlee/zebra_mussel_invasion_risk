	language = {English},
	number = {1},
	journal = {Management of Biological Invasions},
	author = {Whitledge, Gregory W and Weber, Megan M and DeMartini, Jessi and Oldenburg, John and Roberts, Dave and Link, Carolyn and Rackl, Sarahann M and Rude, Neil P and Yung, Andrew J and Bock, Lindsey R and Oliver, Devon C},
