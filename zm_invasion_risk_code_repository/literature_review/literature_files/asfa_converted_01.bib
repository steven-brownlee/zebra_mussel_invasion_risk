	note = {Place: Bristol
Publisher: IOP Publishing},
	keywords = {Community structure, Correlation coefficient, Correlation coefficients, Environmental Studies, Food chains, Food webs, Lakes, Parameters, Plankton, Principal components analysis, Water quality, Zooplankton},
