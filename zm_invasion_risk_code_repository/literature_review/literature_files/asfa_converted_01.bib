	annote = {Last updated - 2024-03-14},
	annote = {SubjectsTermNotLitGenreText - India},
}

@article{zaghloul_impact_2024,
	title = {Impact of some heavy metal accumulation in different organs on fish quality from {Bardawil} {Lake} and human health risks assessment},
	volume = {25},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/scholarly-journals/impact-some-heavy-metal-accumulation-different/docview/2913313794/se-2?accountid=13800},
	doi = {10.1186/s12932-023-00084-2},
	abstract = {Bardawil Lake is a unique aquatic ecosystem that provides a habitat for various fish and other marine organisms. This study aimed to analyze the quality of fish species to prove that this lake is free of pollution, not other Egyptian lakes, due to the accumulation of some heavy metals (Cd, Pb, Cu, and Zn) in various tissues of fish species that were caught from this lake. Thirty-five fish samples were caught during the Spring of 2018 from seven different species: Mugil cephalus, Liza auratus, Sparus aurata, Dicentrarchus labrax, Siganus rivulatus, Anguilla angilla, and Solae solea. The Association of Official Analytical Chemists methods using a spectrophotometer determined the biochemical composition. In contrast, atomic absorption spectrometry (AAS) was employed to determine the heavy metals expressed by µg/g wet weight. Results exposed that the accumulation of essential micronutrient (Cu, Zn) content was higher than toxic elements (Cd \& Pb) in muscles in order to Zn {\textgreater} Cu {\textgreater} Pb {\textgreater} Cd. Muscles {\textless} gills {\textless} liver in order of all metals except Pb with order muscles {\textless} liver {\textless} gills. The metals studied in the muscles were lower than those set by the WHO and the EU standards. The carcinogenic risk with lower allowable limits of 1 × 10–6 to 1 × 10–4 in both normal and high consumption groups; target and total target hazard quotients (THQ \& HI) in muscles were {\textless} 1. The biochemical composition level was highest in the liver, except for protein, which was highest in muscle for all fish species. There is no evidence of harmful contaminants in the muscular tissue of the fish sampled from Bardawil Lake, although fishing activity. However, customers should know that health concerns may be associated with overeating fish.},
	language = {English},
	number = {1},
	journal = {Geochemical Transactions},
	author = {Zaghloul, Ghada Y. and Eissa, Hoda A. and Zaghloul, Amira Y. and Kelany, Mahmoud S. and Hamed, Mohamed A. and Moselhy, Khalid M. El},
	month = dec,
	year = {2024},
	note = {Place: London
Publisher: Springer Nature B.V.},
	keywords = {Absorption spectroscopy, Accumulation, Animal tissues, Aquatic ecosystems, Atomic absorption spectroscopy, Biochemical composition, Body organs, Cadmium, Carcinogens, Composition, Contaminants, Copper, Earth Sciences, Fish, Fishing, Gills, Health risks, Heavy metals, Inland waters, Lakes, Lead, Liver, Marine ecosystems, Marine fish, Marine fishes, Marine organisms, Muscles, Pollution, Risk assessment, Spectral analysis, Spectrometry, Spectrophotometers, Water analysis, Wet weight, Zinc},
	pages = {1},
	annote = {Copyright - © The Author(s) 2024. This work is published under http://creativecommons.org/licenses/by/4.0/ (the “License”). Notwithstanding the ProQuest Terms and Conditions, you may use this content in accordance with the terms of the License.},
