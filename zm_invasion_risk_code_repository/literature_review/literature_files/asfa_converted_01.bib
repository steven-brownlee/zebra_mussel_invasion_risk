	note = {Place: Almería
Publisher: Regional Euro-Asian Biological Invasions Centre},
	keywords = {Acipenser fulvescens, Biology, Dreissena bugensis, Indigenous species, Molluscicides, Mortality, Nontarget organisms, Polychlorinated biphenyls--PCB, Salvelinus namaycush, Statistical analysis, Sturgeon, Trout},
