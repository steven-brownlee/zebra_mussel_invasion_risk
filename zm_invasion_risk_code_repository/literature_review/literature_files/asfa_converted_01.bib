	keywords = {Biomarkers, Biomonitoring, Contaminants, Contamination, Dreissena, Dreissena polymorpha, Invasive species, Mollusks, Nickel, Quagga mussel, Sciences: Comprehensive Works, Tolerance, Zebra mussel},
