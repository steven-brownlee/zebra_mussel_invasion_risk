@article{noauthor_use_2021,
	title = {Use of the {Zebra} {Mussel} {Dreissena} polymorpha ({Mollusca}, {Bivalvia}) as a {Bioindicator} of {Microplastics} {Pollution} in {Freshwater} {Ecosystems}: {A} {Case} {Study} from {Lake} {Iseo} ({North} {Italy})},
	volume = {13},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/scholarly-journals/use-zebra-mussel-dreissena-polymorpha-mollusca/docview/2488510820/se-2?accountid=13800},
	doi = {10.3390/w13040434},
	abstract = {The monitoring of microplastics pollution in freshwater environments trails behind its practice in marine ecosystems. We evaluated the use of the invasive zebra mussel (Dreissena polymorpha) as a potential bioindicator of microplastic litter in freshwater lakes. Samples were collected from three sites (Lovere, Costa Volpino, Castro) at the northern end of Lake Iseo (one of the major subalpine lakes in north Italy) and compared for water physicochemical parameters, biometrical features of zebra mussels, and microplastics items/specimens (color, shape, size, and chemical composition). We hypothesized that since a wastewater treatment plant (WWTP) on the Oglio River discharges into this area of the lake, the microplastics in D.polymorpha could be measured and compared in samples from the three sites at different distances from the WWTP. There was no difference in the physicochemical water parameters and biometric features between the samples from the three sites, whereas there was a significant difference in items/specimen between the sites in decreasing order (mean ± standard deviation): Costa Volpino (0.23 ± 0.43) {\textgreater} Lovere (0.07 ± 0.25) {\textgreater} Castro (0.03 ± 0.18). The chi-square test showed a significant difference in shape, color, and chemical composition frequency in the samples from the three sites. The chemical composition of the microplastics was polyethylene terephthalate (45\%), nylon (20\%), polypropylene (20\%), polyamide resin (10\%), and polyvinyl chloride (5\%). Our data show that the amount of microplastics the zebra mussel accumulated was greater the closer the sampling site to the WWTP. Our findings suggest that the zebra mussel may provide a useful tool to monitor microplastics pollution in lakes.},
	language = {English},
	number = {4},
	journal = {Water},
	year = {2021},
	note = {Place: Basel
Publisher: MDPI AG},
	keywords = {Aquatic ecosystems, Bioindicators, Chemical composition, Chi-square test, Climate change, Color, Colour, Dreissena polymorpha, Ecosystems, environmental monitoring, Environmental monitoring, Environmental Studies, Fourier transforms, Freshwater, freshwater ecosystems, Freshwater ecosystems, Freshwater environments, Freshwater lakes, Freshwater molluscs, Freshwater pollution, Indicator organisms, Inland water environment, Inland waters, Italy, Lago d Iseo, Lakes, Marine ecosystems, Microplastics, Microscopy, Mollusks, Mussels, nylon, Parameters, Physicochemical processes, Plastic pollution, Pollutants, Pollution, Pollution monitoring, Polyamide resins, Polyethylene, polyethylene terephthalate, Polyethylene terephthalate, Polypropylene, Polyvinyl chloride, Shape, Shellfish, Statistical tests, Wastewater treatment, wastewater treatment plant, Wastewater treatment plants, Water pollution},
	pages = {434},
