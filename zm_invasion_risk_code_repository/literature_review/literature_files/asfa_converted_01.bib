Publisher: Nature Publishing Group},
	keywords = {Annotations, Atlases, Crustaceans, Environmental monitoring, Genomes, Gills, Hazardous materials, Hemolymph, Hepatopancreas, Invertebrates, Mass spectrometry, Molecular modelling, Organisms, Peptides, Pollution, Proteins, Proteomics, Sciences: Comprehensive Works, Scientific imaging, Species},
