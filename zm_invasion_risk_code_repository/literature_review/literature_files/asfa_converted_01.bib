@article{mercan_use_2023,
	title = {Use of {Biomonitoring} {Tools} to {Detect} {Water} {Quality}-{Dependent} {Ecosystem} ({Macroinvertebrate}) {Responses} in {Lentic} {Systems}: {The} {Examples} of {Lakes} İznik and {Manyas}, {Türkiye}},
