@mastersthesis{thompson_assessment_2007,
	address = {United States -- New York},
	title = {Assessment of a permanent habitat module in the littoral zone of {Onondaga} {Lake}, {New} {York} with regard to spawning and juvenile habitat of fishes},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/dissertations-theses/assessment-permanent-habitat-module-littoral-zone/docview/304829302/se-2?accountid=13800},
	abstract = {The littoral zone of Onondaga Lake has been perturbed for more than 100 years. Researchers in the early 1990's hypothesized that juvenile fish habitat was limited by a lack of littoral macrophytes and that nesting substrate limitation was due to extensive industrially polluted sediments. A 1993 demonstration project showed that substrate and habitat modifications with a wind-break attracted fish nesting and juvenile fishes. The subsequent installation of a permanent wind-break at a site with a wetland re-connection in 2001 resulted in increased wetland-like habitat, reduced diversity and numbers of macroinvertebrates and had inconclusive results for fishes. Analysis of nest and juvenile fish data for Lepomis spp., largemouth and smallmouth bass found relationships between habitat and densities of nests and juvenile fish. Maps were generated to identify locations of the highest densities of nests and juveniles for use in lake restoration efforts. The construction of more habitat modules is not recommended.},
