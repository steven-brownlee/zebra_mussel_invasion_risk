@phdthesis{bowser_zooplankton_2023,
	address = {United States -- Oklahoma},
	type = {Ph.{D}.},
	title = {Zooplankton {Responses} to {Environmentally} {Relevant} {Concentrations} of {Atrazine} at {Individual}, {Community}, and {Metacommunity} {Scales}},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/dissertations-theses/zooplankton-responses-environmentally-relevant/docview/2909981796/se-2?accountid=13800},
	abstract = {Agricultural disturbances can have considerable negative effects on the biodiversity of surrounding environments. Pesticides have been detected in an overwhelming majority of streams that have been impacted by agriculture in the United States. Atrazine, a chlorotriazine herbicide, is one of the most extensively used pesticides. Atrazine inhibits photosynthesis by interfering with electron transfer and is capable of causing adverse effects in non-target, non-photosynthetic organisms. Few studies have examined how regional dynamics influence zooplankton community response and recovery to herbicide exposure. To do this, we sought to examine the effects of atrazine at individual, community, and metacommunity scales. Metacommunities are defined as a set of local communities that are linked regionally by dispersal of interacting species. First, we examined the effects of atrazine on individual cladocerans with laboratory life-table experiments to determine the lowest observed effect concentrations (LOEC) for atrazine at environmentally relevant concentrations. Second, we observed the effects of regional dispersal on local communities that were affected by atrazine and/or phosphorus. Phosphorus was added to the experiment because surrounding ecosystems can also receive agricultural inputs of fertilizers during rainfall events that may act to mitigate atrazine’s effect on phytoplankton. Third, we constructed metacommunities that represented different proportions of heterogeneity from completely undisturbed to fully disturbed with all three mesocosms exposed to atrazine. Individually, atrazine impacted reproduction, size, and male production in several of the species at environmentally relevant concentrations. Among our species, Daphnia magna was the most sensitive with an atrazine LOEC of 30 µg/L. At the community level, atrazine tended to reduce total zooplankton abundance while phosphorus tended to increase it. However, dispersal helped to increase local zooplankton abundances and likely promoted regional diversity through species sorting and source-sink dynamics. Connectivity, between local and regional scales, within the metacommunity design created similarity between the metacommunity treatments that included disturbed mesocosms. Metacommunities, through dispersal and spatial insurance, tended to curtail the loss of regional species relative to undisturbed and isolated mesocosms. Combined, these chapters highlight the need to include metacommunity dynamics into understanding disturbances on zooplankton communities.},
	language = {English},
	school = {Oklahoma State University},
	author = {Bowser, Robert W.},
	collaborator = {Dzialowski, Andrew},
	year = {2023},
	note = {ISBN: 9798381347111
Publication Title: ProQuest Dissertations and Theses
30575819},
	keywords = {0306:Biology, 0329:Ecology, 0383:Toxicology, 0792:Aquatic sciences, Aquatic sciences, Atrazine, Biology, Cladocerans, Dispersal, Ecology, Metacommunity, Toxicology, Zooplankton communities},
	annote = {Copyright - Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
	annote = {Last updated - 2024-01-19},
}

@article{tomovic_distribution_2023,
	title = {Distribution {Range} of the {Endangered} {Species} {Unio} crassus {Philipsson}, 1788 in {Serbia} ({Western} {Balkans} {Region}), {Historical} and {Recent} {Data}},
	volume = {15},
	url = {http://proxy.lib.sfu.ca/login?url=https://www.proquest.com/scholarly-journals/distribution-range-endangered-species-i-unio/docview/2904927965/se-2?accountid=13800},
	doi = {10.3390/w15244248},
	abstract = {The thick-shelled river mussel, Unio crassus Philipson, 1788, is considered to be one of the species with the highest conservation priority in Serbia. The study represents the first comprehensive research of the distribution of U. crassus in Serbian waters. The research covered a variety of waterbody types throughout Serbia, and distribution data were considered over three time periods from 1953 to 2019. The paper summarizes all the available literature data, field research and information obtained during the review of the collection of malacological material of the Natural History Museum in Belgrade. The results show a positive population trend, which is reflected in an extension of the distribution area and an increase in population density. After reviewing the museum collection, 13 synonyms for U. crassus were identified. The study also revealed a better insight into the habitat requirements and the limiting factors of the species. Substrate characteristics, waterbody types, altitude, and nitrate content of the water seem to be of great importance for the occurrence of the species. The results presented here can improve further measures for the conservation of U. crassus, not only in Serbia, but also in the Western Balkans.},
	language = {English},
	number = {24},
	journal = {Water},
	author = {Tomović, Jelena and Simić, Vladica and Petrović, Ana and Atanacković, Ana and Zorić, Katarina and Paunović, Momir and Raković, Maja},
	year = {2023},
	note = {Place: Basel
Publisher: MDPI AG},
	keywords = {Aquatic ecosystems, Biodiversity, conservation, Creeks \& streams, Discriminant analysis, distribution history, Endangered \& extinct species, Environmental Studies, Europe, freshwater mussels, History, Mollusks, Museums, Nitrates, re-identification, Rivers, Russia, Serbia, Wildlife conservation},
